<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdException name="dss.vector.solutions.etl.dhis2.InvalidFieldException" message="The field [{field}] cannot be exported to DHIS2 at this time." >
        <attributes>
          <char name="field" label="field" required="true" size="2000"/>
        </attributes>
      </mdException>
      
      <mdException name="dss.vector.solutions.etl.dhis2.DHIS2NameLengthException" label="DHIS2 Name Length Exception" message="The name [{name}] is too long. DHIS2 does not support names longer than {charLen} characters.">
        <attributes>
          <char name="name" label="Name" required="true" size="500"/>
          <char name="charLen" label="Char Len" required="true" size="5"/>
        </attributes>
      </mdException>
    </create>
    <update></update>
  </doIt>
  <undoIt>
    <delete>
      <object key="dss.vector.solutions.etl.dhis2.InvalidFieldException" type="com.runwaysdk.system.metadata.MdException" />
    </delete>
  </undoIt>
</version>
