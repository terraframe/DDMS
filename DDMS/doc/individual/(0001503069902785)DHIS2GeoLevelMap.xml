<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdBusiness name="dss.vector.solutions.etl.dhis2.GeoLevelMap" label="DHIS2 GeoMap">
        <attributes>
          <reference type="com.runwaysdk.system.metadata.MdBusiness" name="universal" indexType="unique index" label="Universal" required="true" />
          <reference type="dss.vector.solutions.etl.dhis2.OrgUnitLevel" name="orgUnitLevel" label="Org unit level" required="false" />
          <boolean name="confirmed" label="confirmed" defaultValue="false" />
        </attributes>
      </mdBusiness>
    </create>
    <update>
      <mdException name="dss.vector.solutions.etl.dhis2.response.GeoFieldRequiredException" message="A Geo attribute is required." >
      </mdException>
      
      <mdBusiness name="dss.vector.solutions.etl.dhis2.BasicIdMapping" label="DHIS2 BasicIdMapping">
      </mdBusiness>
      
      <mdBusiness name="dss.vector.solutions.etl.dhis2.GeoMap">
        <attributes>
          <reference name="orgUnit" label="Org Unit" />
        </attributes>
      </mdBusiness>
    </update>
    <delete>
      <object key="dss.vector.solutions.etl.dhis2.OrgUnitLevel.universal" type="com.runwaysdk.system.metadata.MdAttributeReference" />
    </delete>
  </doIt>
  <undoIt>
    <delete></delete>
  </undoIt>
</version>
