<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>

     <mdException
        name="dss.vector.solutions.geo.PoliticalGapException"
        label="Political Gap Exception"
        message="There cannot be a gap in the political hierarchy.">
      </mdException>

     <mdException
        name="dss.vector.solutions.geo.UrbanGapException"
        label="Urban Gap Exception"
        message="There cannot be a gap in the urban hierarchy.">
      </mdException>

     <mdException
        name="dss.vector.solutions.geo.SprayGapException"
        label="Spray Gap Exception"
        message="There cannot be a gap in the spray hierarchy.">
      </mdException>

     <mdException
        name="dss.vector.solutions.geo.PoliticalBranchException"
        label="Political Branch Exception"
        message="The political hierarchy cannot branch.">
      </mdException>

     <mdException
        name="dss.vector.solutions.geo.UrbanBranchException"
        label="Urban Branch Exception"
        message="The urban hierarchy cannot branch.">
      </mdException>

     <mdException
        name="dss.vector.solutions.geo.SprayBranchException"
        label="Spray Branch Exception"
        message="The spray hierarchy cannot branch.">
      </mdException>
    
    </create>
    <update></update>
  </doIt>
  <undoIt>
    <delete>
      <object key="dss.vector.solutions.geo.PoliticalGapException" type="com.runwaysdk.system.metadata.MdException"/>
      <object key="dss.vector.solutions.geo.UrbanGapException" type="com.runwaysdk.system.metadata.MdException"/>
      <object key="dss.vector.solutions.geo.SprayGapException" type="com.runwaysdk.system.metadata.MdException"/>
      <object key="dss.vector.solutions.geo.PoliticalBranchException" type="com.runwaysdk.system.metadata.MdException"/>
      <object key="dss.vector.solutions.geo.UrbanBranchException" type="com.runwaysdk.system.metadata.MdException"/>
      <object key="dss.vector.solutions.geo.SprayBranchException" type="com.runwaysdk.system.metadata.MdException"/>
    </delete>
  </undoIt>
</version>
