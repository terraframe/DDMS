<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdController name="dss.vector.solutions.WhoIsOnlineController">
        <mdAction name="view"></mdAction>
      </mdController>
      
      <mdView name="dss.vector.solutions.WhoIsOnlineView">
        <attributes>
          <char name="username" size="255" label="username" />
          <char name="locale" size="255" label="locale"/>
        </attributes>
        
        <mdMethod name="getViews" label="getViews" static="true" returnType="dss.vector.solutions.WhoIsOnlineView[]" ></mdMethod>
      </mdView>
    </create>
    <update></update>
  </doIt>
  <undoIt>
    <delete>
      <object key="dss.vector.solutions.WhoIsOnlineController" type="com.runwaysdk.system.metadata.MdController"/>
      <object key="dss.vector.solutions.WhoIsOnlineView" type="com.runwaysdk.system.metadata.MdView"/>
    </delete>
  </undoIt>
</version>
