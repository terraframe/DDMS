<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdController
        name="dss.vector.solutions.form.FormObjectController"
        label="FormObject Controller"
        description="The DDMS specific FormObject controller.">
        <mdAction
          name="newInstance"
          label="NewInstance"
          isPost="true">
          <mdParameter
            name="mdFormId"
            label="MdForm Id"
            description="The MdForm id that defines the new FormObject instance."
            order="0"
            type="java.lang.String"/>
        </mdAction>
        <mdAction
          name="createInstance"
          label="CreateInstance"
          isPost="true">
          <mdParameter
            name="formObject"
            label="FormObject"
            description="The FormObject to create."
            order="0"
            type="com.runwaysdk.form.FormObject"/>
        </mdAction>
        <mdAction
          name="formGenerator"
          label="Form Generator"
          description="The entry page to generate forms based on MdForm definitions."
          isPost="false">
        <mdParameter
          name="mdFormId"
          label="MdForm Id"
          description="The MdForm id that defines the new FormObject instance."
          order="0"
          type="java.lang.String"/>
        </mdAction>
      </mdController>
      </create>
    <update>
      
    
    </update>
  </doIt>
  <undoIt>
    <delete>
      <object key="dss.vector.solutions.form.FormObjectController" type="com.runwaysdk.system.metadata.MdController"/>
    </delete>
  </undoIt>
</version>
