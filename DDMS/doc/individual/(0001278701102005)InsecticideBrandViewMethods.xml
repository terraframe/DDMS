<version
	xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<doIt>
		<update>
			<mdView
				name="dss.vector.solutions.irs.InsecticideBrandView">
				<create>
					<mdMethod
						name="getIRSInsecticideBrands"
						static="true"
						returnType="dss.vector.solutions.irs.InsecticideBrandView[]"
						label="Get Insecticide brands for IRS"
						description="Returns all enabled Insecticide Brands for IRS use" />
					<mdMethod
						name="getNozzleInsecticideBrands"
						static="true"
						returnType="dss.vector.solutions.irs.InsecticideBrandView[]"
						label="Get Insecticide brands for Nozzles"
						description="Returns all enabled Insecticide Brands for Nozzle use" />
					<mdMethod
						name="getEfficacyAssayInsecticideBrands"
						static="true"
						returnType="dss.vector.solutions.irs.InsecticideBrandView[]"
						label="Get Insecticide brands for efficacy assays"
						description="Returns all enabled Insecticide Brands for efficacy assay use" />
					<mdMethod
						name="getControlInterventionInsecticideBrands"
						static="true"
						returnType="dss.vector.solutions.irs.InsecticideBrandView[]"
						label="Get Insecticide brands for control intervention"
						description="Returns all enabled Insecticide Brands for control intervention use" />
				</create>
			</mdView>
		</update>
	</doIt>
	<undoIt>
		<delete>
			<object
				key="dss.vector.solutions.irs.InsecticideBrandView.getIRSInsecticideBrands"
				type="com.runwaysdk.system.metadata.MdMethod" />
			<object
				key="dss.vector.solutions.irs.InsecticideBrandView.getNozzleInsecticideBrands"
				type="com.runwaysdk.system.metadata.MdMethod" />
			<object
				key="dss.vector.solutions.irs.InsecticideBrandView.getEfficacyAssayInsecticideBrands"
				type="com.runwaysdk.system.metadata.MdMethod" />
			<object
				key="dss.vector.solutions.irs.InsecticideBrandView.getControlInterventionInsecticideBrands"
				type="com.runwaysdk.system.metadata.MdMethod" />
		</delete>
	</undoIt>
</version>
