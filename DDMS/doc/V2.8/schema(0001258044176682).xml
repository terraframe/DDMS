<version xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create></create>
    <update>
    
      <mdView name="dss.vector.solutions.geo.GeoEntityView">
        <create>
        <attributes>
          <char
            name="moSubType"
            required="false"
            label="MO Subtype"
            size="255" />
        </attributes>
        </create>
      </mdView>

      <mdBusiness
        name="dss.vector.solutions.ontology.BrowserRoot">
        <create>
          <mdMethod
            name="getDefaultGeoRoots"
            label="Get Default Geo Roots"
            static="true"
            returnType="dss.vector.solutions.ontology.BrowserRootView[]">
            <mdParameter
              name="universalType"
              order="0"
              label="Universal Type"
              type="java.lang.String" />
          </mdMethod>
        </create>
      </mdBusiness>
      
      <mdBusiness
        name="dss.vector.solutions.geo.generated.GeoEntity">
        <mdMethod
          name="changeUniversalType"
          returnType="dss.vector.solutions.geo.GeoEntityView">
        </mdMethod>
      </mdBusiness>
      
      <mdBusiness
        name="dss.vector.solutions.ontology.Term">
        <create>
          <attributes>
            <char
            name="display"
            size="255"
            required="true"
            label="Display Label"
            description="Custom Display label or this will be the Term name if empty." />
          </attributes>
        </create>
      </mdBusiness>
    
    </update>
    

    <delete>
    
      <object
        key="Ontology__dss.vector.solutions.geo.generated.GeoEntity.term"
        type="dss.vector.solutions.ontology.BrowserField" />
    
    </delete>
    
  </doIt>
  <undoIt>
    <delete></delete>
  </undoIt>
</version>
