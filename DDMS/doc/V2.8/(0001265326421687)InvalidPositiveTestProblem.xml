<version
  xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdProblem
        name="dss.vector.solutions.InvalidPositiveTestProblem"
        label="Invalid Positive Test Problem"
        description="It is impossible to have more positive tests than tests"
        extends="dss.vector.solutions.NotificationProblem">
        <attributes>
          <integer
            name="amount"
            label="Amount"
            required="true" />
          <integer
            name="amountPositive"
            label="Amount Positive"
            required="true" />
        </attributes>
      </mdProblem>
    </create>
  </doIt>
  <undoIt>
    <delete>
      <object
        key="dss.vector.solutions.InvalidPositiveTestProblem"
        type="com.runwaysdk.system.metadata.MdProblem" />
    </delete>
  </undoIt>
</version>
