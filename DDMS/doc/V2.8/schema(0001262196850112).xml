<version xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
  
    <delete>

      <object
        key="dss.vector.solutions.report.ReportController.generateReport.type"
        type="com.runwaysdk.system.metadata.MdParameter"/>

      <object
        key="dss.vector.solutions.QueryType"
        type="com.runwaysdk.system.metadata.MdEnumeration"/>
        
      <object
        key="dss.vector.solutions.QueryTypeMaster"
        type="com.runwaysdk.system.metadata.MdBusiness"/>    
      
      
    
    </delete>
  
    <create>

      <mdView
        name="dss.vector.solutions.query.AttributeGeoHierarchy"
        label="Qualified Universal"
        description="Class that represents a qualified attribute and GeoHierarchy pairing">
        <attributes>
          <char
            name="attributeDisplayLabel"
            label="Attribute Display Label"
            required="true"
            size="255" />
          <char
            name="geoHierarchyDisplayLabel"
            label="Geo Hierarchy Display Label"
            description="The GeoHierarchy display label, which really maps to the underlying MdBusiness display label."
            required="true"
            size="255" />
          <char
            name="mdAttributeId"
            label="MdAttribute Id"
            required="true"
            size="64" />
          <char
            name="geoHierarchyId"
            label="GeoHierarchy Id"
            required="true"
            size="64" />
        </attributes>
      </mdView>

      <mdException
        name="dss.vector.solutions.query.IncompatibleBoundsException"
        label="Incompatible Bounds Exception"
        description="Thrown when a user tries to mix data types for the lower and upper bound of a range." >
      </mdException>    
    
      <enumerationMaster
        name="dss.vector.solutions.query.RenderTypes"
        label="Render Types">
      </enumerationMaster>
      
      <object
        key="dss.vector.solutions.query.RenderTypes.POINT"
        type="dss.vector.solutions.query.RenderTypes">
        <attribute
          name="enumName"
          value="POINT"/>
        <attributeStruct
          name="displayLabel">
          <attribute
            name="defaultLocale"
            value="Point"/>
        </attributeStruct> 
      </object>
      
      <object
        key="dss.vector.solutions.query.RenderTypes.POLYGON"
        type="dss.vector.solutions.query.RenderTypes">
        <attribute
          name="enumName"
          value="POLYGON"/>
        <attributeStruct
          name="displayLabel">
          <attribute
            name="defaultLocale"
            value="Polygon"/>
        </attributeStruct> 
      </object>
      
      <mdEnumeration
        name="dss.vector.solutions.query.AllRenderTypes"
        type="dss.vector.solutions.query.RenderTypes">
        <includeAll></includeAll>
      </mdEnumeration>    
    
    </create>
    <update>
    
      <mdController
        name="dss.vector.solutions.query.MappingController">
        <create>
          <mdAction
            name="refreshMap"
            isPost="true"
            label="Refresh Map">
            <mdParameter
              name="savedMapId"
              label="SavedMap Id"
              order="0"
              type="java.lang.String" />
          </mdAction>
        </create>
      </mdController>
      
      <mdBusiness
        name="dss.vector.solutions.query.SavedMap">
        <create>
          <mdMethod
            name="refreshMap"
            label="Refesh Map"
            static="false"
            returnType="java.lang.String">
          </mdMethod>
        </create>
      </mdBusiness>    
    
      <mdBusiness
        name="dss.vector.solutions.query.Layer">
        <create>
          <attributes>
            <char
              name="thematicUserAlias"
              label="Thematic Variable"
              description="The user defined alias of the query selectable as a thematic variable"
              required="false"
              size="255" />
            <enumeration
              name="renderAs"
              label="Render As"
              required="true"
              defaultValue="POINT"
              type="dss.vector.solutions.query.AllRenderTypes"
              selectMultiple="false" />
            <reference
              name="geoHierarchy"
              label="Universal Type"
              description="The GeoHierarchy to use as the spatial data for this Layer."
              required="true"
              type="dss.vector.solutions.geo.GeoHierarchy" />
            <reference
              name="mdAttribute"
              label="MdAttribute"
              description="The MdAttribute to join on when defining the layer."
              required="true"
              type="com.runwaysdk.system.metadata.MdAttribute" />
          </attributes>
          <mdMethod
            name="getAttributeGeoHierarchies"
            label="Get AttributeGeoHierarchies"
            description="Returns all AttributeGeoHierarchies selectable for the SavedSearch this Layer references."
            static="false"
            returnType="dss.vector.solutions.query.AttributeGeoHierarchy[]">
          </mdMethod>
        </create>
      </mdBusiness>    

      <mdBusiness
        name="dss.vector.solutions.query.SavedSearch">
        <create>
          <mdMethod
            name="getAttributeGeoHierarchies"
            label="Get AttributeGeoHierarchies"
            description="Returns all AttributeGeoHierarchies selectable for this SavedSearch."
            static="false"
            returnType="dss.vector.solutions.query.AttributeGeoHierarchy[]">
          </mdMethod>
        </create>
      </mdBusiness>

      <mdBusiness
        name="dss.vector.solutions.query.Styles">
        <attributes>
          <char
            name="fill"
            label="Font Color"/>
        </attributes>
      </mdBusiness>
      
      <mdBusiness
        name="dss.vector.solutions.geo.generated.GeoEntity">
        <attributes>
          <reference
          name="term"
          label="Sub-Type"/>
        </attributes>
      </mdBusiness>      

      <mdBusiness
        name="dss.vector.solutions.query.SavedSearch">
        <attributes>
          <char
            name="queryType"
            size="255"/>
        </attributes>
        <create>
          <mdMethod
            name="getThematicVariables"
            static="false"
            label="Get Thematic Variables"
            description="Returns all available thematic variables for the underlying query."
            returnType="dss.vector.solutions.query.ThematicVariable[]">
          </mdMethod>
        </create>
      </mdBusiness>

    </update>
  </doIt>
  <undoIt>
    <delete></delete>
  </undoIt>
</version>
