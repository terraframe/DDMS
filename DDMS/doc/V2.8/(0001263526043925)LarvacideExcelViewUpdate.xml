<version
  xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <update>
      <mdView
        name="dss.vector.solutions.export.LarvacideExcelView"
        label="Larvacide Excel View"
        description="Excel view for importing/exporting control of immatures">
        <attributes>
          <virtual
            name="natureOfControl"
            concrete="natureOfControl"
            label="Nature of control activity"
            description="Routine Control or Emergency control"
            type="dss.vector.solutions.intervention.monitor.Larvacide"/>
        </attributes>
        <create>
          <attributes>
            <char
              name="substance"
              label="Substance"
              size="255"/>
          </attributes>
        </create>
      </mdView>
    </update>
  </doIt>
  <undoIt>
    <delete>
      <object
        key="dss.vector.solutions.export.LarvacideExcelView.substance"
        type="com.runwaysdk.system.metadata.MdAttributeCharacter" />
    </delete>
  </undoIt>
</version>
