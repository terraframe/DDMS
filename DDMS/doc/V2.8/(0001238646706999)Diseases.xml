<version
  xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdBusiness
        name="dss.vector.solutions.general.Disease"
        label="Disease"
        generateController="false"
        cacheAlgorithm="everything">
        <attributes>
          <reference
            name="dimension"
            label="Dimension"
            type="com.runwaysdk.system.metadata.MdDimension"
            required="true" />
          <reference
            name="menuRoot"
            label="Menu Root"
            required="false"
            description="A menu term"
            type="dss.vector.solutions.ontology.Term" />
        </attributes>
        <mdMethod
          name="getMenuJson"
          label="Get Menu JSON"
          description="Returns the JSON to create the menu"
          static="true"
          returnType="java.lang.String">
        </mdMethod>
      </mdBusiness>

      <object
        key="MALARIA"
        type="dss.vector.solutions.general.Disease">
        <attributeReference
          name="dimension"
          key="Dimension.MALARIA" />
      </object>

      <object
        key="DENGUE"
        type="dss.vector.solutions.general.Disease">
        <attributeReference
          name="dimension"
          key="Dimension.DENGUE" />
      </object>
    </create>
  </doIt>
  <undoIt>
    <delete>
      <object
        key="dss.vector.solutions.general.Disease"
        type="com.runwaysdk.system.metadata.MdBusiness" />
    </delete>
  </undoIt>
</version>
