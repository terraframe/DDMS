        type="dss.vector.solutions.QueryTypeMaster"
        id="Entomology">
        <valueAttribute
          name="enumName"
          value="RESISTANCE"/>
        <structAttribute
          name="displayLabel">
          <valueAttribute
            name="defaultLocale"
            value="Resistance"/>
        </structAttribute>
      </object>
    </create>
    <create>
      <object
        type="dss.vector.solutions.QueryTypeMaster"
        id="Efficacy">
