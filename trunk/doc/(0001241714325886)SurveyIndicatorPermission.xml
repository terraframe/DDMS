        <mdRelationshipPermission type="dss.vector.solutions.intervention.monitor.HouseholdPerson">
          <operation name="READ"/>
          <operation name="READ_PARENT"/>
          <operation name="READ_CHILD"/>
          <operation name="READ_ALL"/>
        </mdRelationshipPermission>
        <mdViewPermission type="dss.vector.solutions.intervention.monitor.PersonView">
          <operation name="READ"/>
          <operation name="READ_ALL"/>
          <operation name="CREATE"/>
          <operation name="WRITE_ALL"/>
          <operation name="WRITE_ALL"/>
