<version
  xsi:noNamespaceSchemaLocation="../profiles/version_gis.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>

      <mdTree
        name="dss.vector.solutions.entomology.CollectionMosquito"
        label="Collection-Mosquito"
        generateController="false">
        <parent
          cardinality="1"
          name="dss.vector.solutions.entomology.AbstractMosquitoCollection"
          method="Collections" />
        <child
          cardinality="*"
          name="dss.vector.solutions.entomology.Mosquito"
          method="Mosquitos" />
      </mdTree>
      <mdView
        name="dss.vector.solutions.entomology.MosquitoCollectionPointView"
        label="Mosquito Collection Point"
        extends="dss.vector.solutions.entomology.MorphologicalSpecieGroupView">
        <attributes>
          <integer
            name="total"
            label="Total"
            description="Total Number of mosquitos on which assays have been performed"
            required="true"
            default_value="0" />
          <virtual
            name="dateCollected"
            required="true"
            type="dss.vector.solutions.entomology.ConcreteMosquitoCollection"
            concrete="dateCollected" />
          <virtual
            name="geoEntity"
            required="true"
            type="dss.vector.solutions.entomology.ConcreteMosquitoCollection"
            concrete="geoEntity" />
        </attributes>
        <mdMethod
          name="searchByGeoEntityAndDate"
          label="Search By GeoEntity and Date"
          description="Searches for all morphological specie group views on any mosquito collection between the given start and end date for a given geo entity"
          static="true"
          returnType="dss.vector.solutions.entomology.MosquitoCollectionPointView[]">
          <mdParameter
            name="geoId"
            label="Geo Id"
            type="java.lang.String"
            order="0"
            description="Geo Id" />
          <mdParameter
            name="startDate"
            label="Start Date"
            type="java.util.Date"
            order="1" />
          <mdParameter
            name="endDate"
            label="End Date"
            type="java.util.Date"
            order="2" />
        </mdMethod>
        <mdMethod
          name="getView"
          returnType="dss.vector.solutions.entomology.MosquitoCollectionPointView"
          label="Get View"
          static="true">
          <mdParameter
            name="id"
            label="id"
            order="0"
            type="java.lang.String" />
        </mdMethod>
        <mdMethod
          name="lockView"
          returnType="dss.vector.solutions.entomology.MosquitoCollectionPointView"
          label="Lock View"
          static="true">
          <mdParameter
            name="id"
            label="id"
            order="0"
            type="java.lang.String" />
        </mdMethod>
        <mdMethod
          name="unlockView"
          returnType="dss.vector.solutions.entomology.MosquitoCollectionPointView"
          label="unlock View"
          static="true">
          <mdParameter
            name="id"
            label="id"
            order="0"
            type="java.lang.String" />
        </mdMethod>
      </mdView>
    </create>
  </doIt>
  <undoIt>
    <delete></delete>
  </undoIt>
</version>