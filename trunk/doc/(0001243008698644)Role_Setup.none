        id="dataCapturer">
        <valueAttribute
          name="roleName"
          value="dataCapturer" />
        <structAttribute
          name="displayLabel">
          <valueAttribute
            name="defaultLocale"
            value="Data Capturer" />
        </structAttribute>
      </object>
      <object
        id="testDataCapturer"
        type="com.terraframe.mojo.system.Users">
        <valueAttribute
          name="username"
          value="testDataCapturer" />
        <valueAttribute
          name="password"
          value="mdsstest2" />
