      </object>
      <object
        id="testManager"
        type="com.terraframe.mojo.system.Users">
        <valueAttribute
          name="username"
          value="testManager" />
        <valueAttribute
          name="password"
          value="mdsstest2" />
        <valueAttribute
          name="sessionLimit"
          value="10" />
      </object>
      <object
        type="com.terraframe.mojo.system.Roles"
        id="dataCapturer">
        <valueAttribute
          name="roleName"
          value="GUIVisibility" />
        <structAttribute
          name="displayLabel">
          <valueAttribute
            name="defaultLocale"
            value="GUIVisibility" />
        </structAttribute>
      </object>
    </create>
    <permissions>
      <role
        roleName="manager">
        <superRole roleName="GUIVisibility"/>
      </role>
      <role
        roleName="entomologist">
        <superRole roleName="GUIVisibility"/>
      </role>
      <role
        roleName="dataCapturer">
