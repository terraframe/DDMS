<version
  xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <update>
      <mdBusiness
        name="dss.vector.solutions.geo.generated.GeoEntity">
        <create>
          <attributes>
            <localChar
              name="entityLabel"
              required="false"
              label="Geo Entity Name"
              size="255" />
          </attributes>
        </create>
      </mdBusiness>
      <mdException
        name="dss.vector.solutions.query.GeoEntityNotUniqueException">
        <create>
          <attributes>
            <char
              name="entityLabel"
              label="Entity name"
              size="255" />
          </attributes>
        </create>
      </mdException>
      <mdView
        name="dss.vector.solutions.geo.GeoEntityView">
        <create>
          <attributes>
            <char
              name="entityLabel"
              label="Entity name"
              size="255"
              required="true" />
          </attributes>
        </create>
      </mdView>
      <mdView
        name="dss.vector.solutions.irs.GeoTargetView">
        <create>
          <attributes>
            <char
              name="entityLabel"
              label="Entity name"
              size="255"
              required="true" />
          </attributes>
        </create>
      </mdView>
      <mdView
        name="dss.vector.solutions.export.GeoEntityExcelView">
        <create>
          <attributes>
            <char
              name="entityLabel"
              label="Entity name"
              size="255" />
          </attributes>
        </create>
      </mdView>

    </update>
  </doIt>
  <undoIt>
    <delete>
    
    </delete>
  </undoIt>
</version>
