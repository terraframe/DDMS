<version
	xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <update>
      <mdController name="dss.vector.solutions.kaleidoscope.DataSetController">
        <mdAction name="getAll" isPost="false" />
        <create>        
          <mdAction name="management" label="Management" isPost="false"></mdAction>
        </create>
      </mdController>
      <mdBusiness name="dss.vector.solutions.kaleidoscope.MappableClass">
        <create>
          <attributes>
            <boolean name="removable" label="Removable" negativeLabel="False" positiveLabel="True"/>
          </attributes>
        </create>
      </mdBusiness>
    </update>
    <create>
      <mdException name="dss.vector.solutions.kaleidoscope.RemoveMappableClassException" message="A form defined by a dataset cannot be removed independent of the dataset" />
      <mdException name="dss.vector.solutions.kaleidoscope.ModifyFormException" message="A form defined by a dataset cannot have attributes added or removed" />
    </create>
  </doIt>
  <undoIt>
    <delete>
      <object key="dss.vector.solutions.kaleidoscope.DataSetController.management" type="com.runwaysdk.system.metadata.MdAction"/>
    </delete>
  </undoIt>
</version>