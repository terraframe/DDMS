<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <update>
      <mdBusiness name="dss.vector.solutions.basemap.OfflineBasemapManagement">
        <create>
          <attributes>
             <char name="fileName" label="File name" required="true" size="250" />
             <char name="filePath" label="File path" required="true" size="1000" />
             <boolean name="quedForUpload" label="Qued for upload" required="true" defaultValue="false" />
             <boolean name="uploadSuccessful" label="File uploaded successfully" required="true" defaultValue="false" />
          </attributes>
          <mdMethod name="importBasemapFiles" label="Import basemap files" static="true" returnType="java.lang.Boolean">
            <mdParameter name="files" label="Files" order="0" type="java.lang.String[]" />
          </mdMethod>
        </create>
      </mdBusiness>
    </update>
  </doIt>
  <undoIt>
    <delete></delete>
  </undoIt>
</version>
