<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <update>
      <mdView
        name="dss.vector.solutions.export.IndividualCaseExcelView"
        label="Individual Case Excel View"
        description="Excel view for individual cases"
        publish="false">
        <create>
        <attributes>
          <virtual
            concrete="identifier"
            type="dss.vector.solutions.Person"/>
          <virtual
            concrete="otherSettlements"
            type="dss.vector.solutions.intervention.monitor.IndividualCase"/>
          <char
            name="origin"
            label="Origin of Infection"
            size="255"/>
          <virtual
            concrete="plasmaLeakageOnset"
            type="dss.vector.solutions.intervention.monitor.IndividualCase"/>
          <virtual
            concrete="hemorrhagicOnset"
            type="dss.vector.solutions.intervention.monitor.IndividualCase"/>
          <virtual
            concrete="caseIdentifier"
            type="dss.vector.solutions.intervention.monitor.IndividualInstance"/>
          <char
            name="diagnosisType"
            label="Type of Diagnosis"
            size="255"/>
          <char
            name="diagnosis"
            label="Clinical diagnosis"
            size="255"/>
          <char
            name="confirmedDiagnosis"
            label="Confirmed diagnosis"
            size="255"/>
          <virtual
            concrete="confirmedDiagnosisDate"
            type="dss.vector.solutions.intervention.monitor.IndividualInstance"/>
          <virtual
            concrete="dateOfDeath"
            type="dss.vector.solutions.intervention.monitor.IndividualInstance"/>
          <char
            name="classification"
            label="Sample classification"
            size="255"/>
          <char
            name="testResult"
            label="Test result"
            size="255"/>
          <char
            name="primaryInfection"
            label="Primary or sequential infection"
            size="255"/>
          <!-- Physician -->
          <virtual
            concrete="identifier"
            name="physicianIdentifier"
            type="dss.vector.solutions.Person"/>
          <virtual
            concrete="firstName"
            name="physicianFirstName"
            type="dss.vector.solutions.Person"/>
          <virtual
            concrete="lastName"
            name="physicianLastName"
            type="dss.vector.solutions.Person"/>
          <virtual
            concrete="dateOfBirth"
            name="physicianDateOfBirth"
            type="dss.vector.solutions.Person"/>
          <char
            name="physicianSex"
            label="Physician Sex"
            size="255"/>
          <reference
            name="physicianResidence"
            label="Physician Residence"
            type="dss.vector.solutions.geo.generated.GeoEntity"/>
          <reference
            name="physicianWorkplace"
            label="Physician Workplace"
            type="dss.vector.solutions.geo.generated.GeoEntity"/>
        </attributes>
        </create>
      </mdView>
    </update>
  </doIt>
  <undoIt>
    <delete>
    </delete>
  </undoIt>
</version>
