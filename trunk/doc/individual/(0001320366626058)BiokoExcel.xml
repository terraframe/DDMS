<version
  xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdException
        name="dss.vector.solutions.generator.UnsupportedImportTypeException"
        label="Unsupported import type exception"
        message="The type [{classType}] is not a supported import type of this import screen">
        <attributes>
          <text
            name="classType"
            label="Class type" />
        </attributes>
      </mdException>    
      <mdException
        name="dss.vector.solutions.general.UnknownValueException"
        label="Unknown value exception"
        message="Unable to find a(n) {typeLabel} with a {attributeLabel} of [{value}]">
        <attributes>
          <text
            name="typeLabel"
            label="Type" />
          <text
            name="attributeLabel"
            label="Attribute" />
          <text
            name="value"
            label="Value" />
        </attributes>
      </mdException>
      <mdException
        name="dss.vector.solutions.form.HouseholdMismatchException"
        label="Household Mismatch Exception"
        message="The bednet [{netId}] on person [{personId}] is not under the household [{householdId}]">
        <attributes>
          <text
            name="householdId"
            label="Household id" />
          <text
            name="netId"
            label="Net id" />
          <text
            name="personId"
            label="Person id" />
        </attributes>
      </mdException>
    </create>
    <update>
      <mdBusiness
        name="dss.vector.solutions.form.business.FormSurvey"
        label="Survey"
        description="Form survey">
        <create>
          <mdMethod
            name="excelExport"
            label="Excel Export"
            returnType="java.io.InputStream"
            static="true" />
          <mdMethod
            name="excelImport"
            label="Excel Import"
            returnType="java.io.InputStream"
            static="true">
            <mdParameter
              name="stream"
              label="stream"
              order="0"
              type="java.io.InputStream" />
          </mdMethod>
        </create>
      </mdBusiness>
      <mdController
        name="dss.vector.solutions.generator.ExcelController"
        label="Excel Controller">
        <create>
          <mdAction
            name="surveyExcelExport"
            label="Survey excel export"
            isPost="false" />
          <mdAction
            name="surveyImportType"
            label="Import Type"
            isPost="false" />
          <mdAction
            name="surveyExcelImport"
            label="Excel Import"
            isPost="true" />
        </create>
      </mdController>
    </update>
  </doIt>
  <undoIt>
    <delete>
      <object
        key="dss.vector.solutions.generator.UnsupportedImportTypeException"
        type="com.runwaysdk.system.metadata.MdException" />    
      <object
        key="dss.vector.solutions.form.HouseholdMismatchException"
        type="com.runwaysdk.system.metadata.MdException" />
      <object
        key="dss.vector.solutions.general.UnknownValueException"
        type="com.runwaysdk.system.metadata.MdException" />
      <object
        key="dss.vector.solutions.generator.ExcelController.surveyExcelExport"
        type="com.runwaysdk.system.metadata.MdAction" />
      <object
        key="dss.vector.solutions.generator.ExcelController.surveyImportType"
        type="com.runwaysdk.system.metadata.MdAction" />
      <object
        key="dss.vector.solutions.generator.ExcelController.surveyExcelImport"
        type="com.runwaysdk.system.metadata.MdAction" />
      <object
        key="dss.vector.solutions.form.business.FormSurvey.excelExport"
        type="com.runwaysdk.system.metadata.MdMethod" />
      <object
        key="dss.vector.solutions.form.business.FormSurvey.excelImport"
        type="com.runwaysdk.system.metadata.MdMethod" />
    </delete>
  </undoIt>
</version>