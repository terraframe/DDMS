<version xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <update>
      <mdView
        name="dss.vector.solutions.export.AggregatedPremiseExcelView">
        <create>
          <attributes>
            <virtual
              concrete="premisesAvailable"
              type="dss.vector.solutions.intervention.monitor.AggregatedPremiseVisit"/>
            <virtual
              concrete="premisesIncluded"
              type="dss.vector.solutions.intervention.monitor.AggregatedPremiseVisit"/>
          </attributes>
        </create>
      </mdView>
      <mdView
        name="dss.vector.solutions.export.TeamSprayExcelView"
        publish="false">
        <create>
          <attributes>
            <virtual
              concrete="operatorTarget"
              type="dss.vector.solutions.irs.OperatorSprayStatus"/>
          </attributes>
        </create>
      </mdView>
    </update>
    <create>
      <mdException
        name="dss.vector.solutions.export.DuplicateOperatorSprayImportException"
        label="Duplicate Operator Spray Import Exception"
        description="Thrown when a user attempts to import an operatory spray that already exists in the system"
        message="Operator Spray already exists with Spray Date={sprayDate}, Operator ID={operatorId}, Insecticide={insecticideTerm}, Spray Method={sprayMethod}, Household ID={householdId}, and Structure ID={structureId}">
        <attributes>
          <date
            name="sprayDate"
            label="Spray Date"/>
          <char
            name="operatorId"
            label="Operator ID"
            size="255"/>
          <char
            name="insecticideTerm"
            label="Insecticide Term"
            size="255"/>
          <char
            name="sprayMethod"
            label="Spray Method"
            size="255"/>
          <char
            name="householdId"
            label="Household ID"
            size="255"/>
          <char
            name="structureId"
            label="Structure ID"
            size="255"/>
        </attributes>
      </mdException>
      <mdException
        name="dss.vector.solutions.export.DuplicateTeamSprayImportException"
        label="Duplicate Team Spray Import Exception"
        description="Thrown when a user attempts to import a team spray that already exists in the system"
        message="Team Spray already exists with Spray Date={sprayDate}, Spray Team={sprayTeam}, Insecticide={insecticideTerm}, Spray Method={sprayMethod}, and Operator ID={operatorId}">
        <attributes>
          <date
            name="sprayDate"
            label="Spray Date"/>
          <char
            name="sprayTeam"
            label="Spray Team"
            size="255"/>
          <char
            name="insecticideTerm"
            label="Insecticide Term"
            size="255"/>
          <char
            name="sprayMethod"
            label="Spray Method"
            size="255"/>
          <char
            name="operatorId"
            label="Operator ID"
            size="255"/>
        </attributes>
      </mdException>
      <mdException
        name="dss.vector.solutions.export.DuplicateZoneSprayImportException"
        label="Duplicate Zone Spray Import Exception"
        description="Thrown when a user attempts to import a team spray that already exists in the system"
        message="Zone Spray already exists with Spray Date={sprayDate}, Geo Entity={geoEntity}, Insecticide={insecticideTerm}, Spray Method={sprayMethod}, and Spray Team={sprayTeam}">
        <attributes>
          <date
            name="sprayDate"
            label="Spray Date"/>
          <reference
            name="geoEntity"
            label="Geo Entity"
            type="dss.vector.solutions.geo.generated.GeoEntity"/>
          <char
            name="insecticideTerm"
            label="Insecticide Term"
            size="255"/>
          <char
            name="sprayMethod"
            label="Spray Method"
            size="255"/>
          <char
            name="sprayTeam"
            label="Spray Team"
            size="255"/>
        </attributes>
      </mdException>
    </create>
  </doIt>
  <undoIt>
    <delete>
      <object
        key="dss.vector.solutions.export.AggregatedPremiseExcelView.premisesAvailable"
        type="com.runwaysdk.system.metadata.MdAttributeVirtual"/>
      <object
        key="dss.vector.solutions.export.AggregatedPremiseExcelView.premisesIncluded"
        type="com.runwaysdk.system.metadata.MdAttributeVirtual"/>
      <object
        key="dss.vector.solutions.export.TeamSprayExcelView.operatorTarget"
        type="com.runwaysdk.system.metadata.MdAttributeVirtual"/>
      <object
        key="dss.vector.solutions.export.DuplicateOperatorSprayImportException"
        type="com.runwaysdk.system.metadata.MdException"/>
      <object
        key="dss.vector.solutions.export.DuplicateTeamSprayImportException"
        type="com.runwaysdk.system.metadata.MdException"/>
      <object
        key="dss.vector.solutions.export.DuplicateZoneSprayImportException"
        type="com.runwaysdk.system.metadata.MdException"/>
    </delete>
  </undoIt>
</version>