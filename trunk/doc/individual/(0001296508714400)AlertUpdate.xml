<version
  xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <delete>
      <object
        key="dss.vector.solutions.general.OutbreakAlert.message"
        type="com.runwaysdk.system.metadata.MdAttribute" />
    </delete>
    <update>
      <mdInformation
        name="dss.vector.solutions.general.OutbreakAlert"
        message="${messageText}.  {emailFailure}">
        <create>
          <attributes>
            <text
              name="messageText"
              label="Message Text"
              size="255" />
          </attributes>
        </create>
      </mdInformation>
    </update>
  </doIt>
  <undoIt>
    <delete></delete>
  </undoIt>
</version>
