<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create></create>
    <update>
      <mdController
        name="dss.vector.solutions.form.MdFormAdminController">
        <create>
          <mdAction 
            name="existingForms"
            label="Existing Forms"
            description="Returns a list of existing MdWebForms."
            isPost="true">
          </mdAction>
          <mdAction
            name="fetchFormAttributes"
            label="Fetch Form Attributes"
            description="Retrieves the attributes of a form so they can be pushed to the admin page"
            isPost="true">
            <mdParameter name="formId" label="Form id" description="Existing MdForm id" order="0" type="java.lang.String" />
          </mdAction>
        </create>
      </mdController>
    </update>
  </doIt>
  <undoIt>
    <delete>
      <object
        key="dss.vector.solutions.form.MdFormAdminController.existingForms" 
        type="com.runwaysdk.system.metadata.MdAction"/>
      <object
        key="dss.vector.solutions.form.MdFormAdminController.fetchFormAttributes" 
        type="com.runwaysdk.system.metadata.MdAction"/>
    </delete>
  </undoIt>
</version>