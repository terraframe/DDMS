  xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>     
      <mdInformation name="dss.vector.solutions.query.IgnoredColumnInformation" label="Ignored column information" message="Column [{columnName}] has been removed as persisted indicators do not aggregate. Add the indicator in the dataset manager instead.">
