          <mdMethod
            name="getMappableSearches"
            label="Get Mappable Searches"
            description="Returns all saved searches that can be mapped."
            static="true"
            returnType="dss.vector.solutions.query.SavedSearchViewQuery">
          </mdMethod>
        </create>
      
