<version
  xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>

      <mdBusiness
        name="dss.vector.solutions.intervention.monitor.ITNHouseholdSurvey"
        label="ITN household survey"
        generateController="true">
        <attributes>
          <date
            name="startDate"
            label="Start Date"
            required="true" />
          <date
            name="endDate"
            label="End Date"
            required="true" />
          <reference
            name="surveyLocation"
            label="Survey Location"
            type="dss.vector.solutions.geo.generated.GeoEntity" />
          <char
            name="agentFirstName"
            label="Agent/Interviewer name"
            size="255" />
          <char
            name="agentSurname"
            label="Agent/Interviewer surname"
            size="255" />
          <char
            name="questionnaireNumber"
            label="Questionnaire number"
            size="255" />
          <integer
            name="residents"
            label="Number of people residing in household"
            rejectNegative="true" />
          <integer
            name="pregnantResidents"
            label="Number of pregnant women residing in household"
            rejectNegative="true" />
          <integer
            name="childResidents"
            label="Number of children less than 5 years old residing in household"
            rejectNegative="true" />
          <integer
            name="itns"
            label="Number of ITNs found in household"
            rejectNegative="true" />
          <integer
            name="damagedItns"
            label="Number of ITNs damaged to a point where they are no longer effective to prevent vector contact"
            rejectNegative="true" />
          <integer
            name="hangingItns"
            label="Number of ITNs hanging for purpose of preventing mosquito bites"
            rejectNegative="true" />
          <integer
            name="otherItns"
            label="Number of ITNs used for purpose other than preventing mosquito bites"
            rejectNegative="true" />
          <integer
            name="monthReceived"
            label="When were the ITNs received (month and/or year)"
            rejectNegative="true" />
          <integer
            name="yearReceived"
            label="Year"
            rejectNegative="true" />
          <integer
            name="usedItns"
            label="The number of ITNs used by household head"
            rejectNegative="true" />
          <boolean
            name="usedEveryNight"
            label="Are the ITNs used every night"
            positiveLabel="Yes"
            negativeLabel="No" />
          <boolean
            name="netsObtained"
            label="The method how ITNs were obtained"
            positiveLabel="Free"
            negativeLabel="Bought" />
          <reference
            name="freeProvider"
            label="Free Net Provider"
            type="dss.vector.solutions.ontology.Term" />
          <reference
            name="boughtProvider"
            label="Bought Net Provider"
            type="dss.vector.solutions.ontology.Term" />
          <reference
            name="washed"
            label="ITNs have ever been washed"
            type="dss.vector.solutions.ontology.Term" />
          <integer
            name="washFrequency"
            label="ITNs wash frequency per interval"
            rejectNegative="true" />
          <reference
            name="washInterval"
            label="ITNs wash interval"
            type="dss.vector.solutions.ontology.Term" />
          <boolean
            name="retreated"
            label="ITNs have ever been re-treated"
            positiveLabel="Yes"
            negativeLabel="No" />
          <reference
            name="retreatmentPeriod"
            label="Last time of re-treatment"
            type="dss.vector.solutions.ontology.Term" />
        </attributes>
        <mdMethod
          name="getView"
          label="Get View"
          returnType="dss.vector.solutions.intervention.monitor.ITNHouseholdSurveyView"
          static="true">
          <mdParameter
            name="id"
            label="id"
            order="0"
            type="java.lang.String" />
        </mdMethod>
        <mdMethod
          name="lockView"
          label="lock View"
          returnType="dss.vector.solutions.intervention.monitor.ITNHouseholdSurveyView"
          static="false" />
        <mdMethod
          name="unlockView"
          label="unlock View"
          returnType="dss.vector.solutions.intervention.monitor.ITNHouseholdSurveyView"
          static="false" />
      </mdBusiness>

      <mdTree
        name="dss.vector.solutions.intervention.monitor.ITNHouseholdSurveyNonUseReason"
        label="ITN Community Non Use Reason Grid"
        generateController="false">
        <attributes>
          <integer
            name="amount"
            label="Reason for not using ITN"
            defaultValue="0"
            rejectNegative="true" />
        </attributes>
        <parent
          cardinality="*"
          name="dss.vector.solutions.intervention.monitor.ITNHouseholdSurvey"
          method="ITNHouseholdSurveysWithNonUseReasons" />
        <child
          cardinality="*"
          name="dss.vector.solutions.ontology.Term"
          method="NonUseReasons" />
      </mdTree>

      <mdTree
        name="dss.vector.solutions.intervention.monitor.ITNHouseholdSurveyTargetGroup"
        label="ITN Community Target Group"
        generateController="false">
        <attributes>
          <integer
            name="amount"
            label="Use by target group last night"
            defaultValue="0"
            required="true"
            rejectNegative="true" />
        </attributes>
        <parent
          cardinality="*"
          name="dss.vector.solutions.intervention.monitor.ITNHouseholdSurvey"
          method="ITNHouseholdSurveysWithTargetGroups" />
        <child
          cardinality="*"
          name="dss.vector.solutions.ontology.Term"
          method="TargetGroups" />
      </mdTree>

      <mdTree
        name="dss.vector.solutions.intervention.monitor.ITNHouseholdSurveyNet"
        label="ITN Community Net"
        generateController="false">
        <attributes>
          <integer
            name="amount"
            label="The ITN type used"
            required="true"
            rejectNegative="true"
            defaultValue="0" />
        </attributes>
        <parent
          cardinality="*"
          name="dss.vector.solutions.intervention.monitor.ITNHouseholdSurvey"
          method="ITNHouseholdSurveysWithNets" />
        <child
          cardinality="*"
          name="dss.vector.solutions.ontology.Term"
          method="Nets" />
      </mdTree>

