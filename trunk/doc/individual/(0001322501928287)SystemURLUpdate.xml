<version
  xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <update>
      <mdBusiness
        name="dss.vector.solutions.general.SystemURL">
        <create>
          <attributes>
            <char
              name="urlName"
              label="URL name"
              size="1024"
              required="true" />
          </attributes>
          <mdMethod
            name="getURLs"
            label="Get URLs"
            static="true"
            returnType="dss.vector.solutions.general.SystemURLQuery" />
        </create>
      </mdBusiness>
    </update>
  </doIt>
  <undoIt>
    <delete>
      <object
        key="dss.vector.solutions.general.SystemURL.getURLs"
        type="com.runwaysdk.system.metadata.MdMethod" />
      <object
        key="dss.vector.solutions.general.SystemURL.urlName"
        type="com.runwaysdk.system.metadata.MdAttribute" />
    </delete>
  </undoIt>
</version>
