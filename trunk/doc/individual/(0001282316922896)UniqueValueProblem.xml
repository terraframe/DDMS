<version
  xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdProblem
        name="dss.vector.solutions.general.UniqueValueProblem"
        label="Unique Value Problem"
        message="{displayLabel} already has an instance with the value of [{value}]">
        <attributes>
          <char
            name="displayLabel"
            label="Display Label"
            size="255" />
          <char
            name="value"
            label="Value"
            size="255" />
        </attributes>
      </mdProblem>
    </create>
  </doIt>
  <undoIt>
    <delete>
      <object
        key="dss.vector.solutions.general.UniqueValueProblem"
        type="com.runwaysdk.system.metadata.MdProblem" />
    </delete>
  </undoIt>
</version>