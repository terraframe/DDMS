<version xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdProblem
        name="dss.vector.solutions.export.InvalidEnumProblem"
        label="Invalid Enumeration Problem"
        description="Thrown when the text value on an enum column in an import doesn't match any instances"
        message="[{enumName}] is not a valid [{enumLabel}]">
        <attributes>
          <char
            name="enumName"
            label="Enumeration Name"
            size="255"/>
          <char
            name="enumLabel"
            label="Enumeration Label"
            size="255"/>
        </attributes>
      </mdProblem>
    </create>
  </doIt>
  <undoIt>
    <delete>
      <object
        key="dss.vector.solutions.export.InvalidEnumProblem"
        type="com.runwaysdk.system.metadata.MdProblem"/>
    </delete>
  </undoIt>
</version>