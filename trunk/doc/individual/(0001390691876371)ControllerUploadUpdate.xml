<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <update>
      <mdController name="dss.vector.solutions.generator.ExcelController">
        <mdAction name="excelImport">
          <create>
            <mdParameter name="excelType" label="Excel type" order="0" type="java.lang.String" />
            <mdParameter name="upfile" label="File" order="1" type="com.runwaysdk.controller.MultipartFileParameter" />
          </create>
        </mdAction>
        <mdAction name="surveyExcelImport">
          <create>
            <mdParameter name="type" label="Type" order="0" type="java.lang.String" />
            <mdParameter name="upfile" label="File" order="1" type="com.runwaysdk.controller.MultipartFileParameter" />
          </create>
        </mdAction>
      </mdController>
      <mdController name="dss.vector.solutions.query.MappingController">
        <mdAction name="uploadMapImage">
          <create>
            <mdParameter name="imageFile" label="File" order="0" type="com.runwaysdk.controller.MultipartFileParameter" />
          </create>
        </mdAction>
      </mdController>
      <mdController name="dss.vector.solutions.query.QueryController">
        <mdAction name="uploadTemplate">
          <create>
            <mdParameter name="templateFile" label="File" order="1" type="com.runwaysdk.controller.MultipartFileParameter" />
          </create>
        </mdAction>
      </mdController>
      <mdController name="dss.vector.solutions.util.LocalizationController">
        <mdAction name="importFile">
          <create>
            <mdParameter name="upfile" label="File" order="0" type="com.runwaysdk.controller.MultipartFileParameter" />
          </create>
        </mdAction>
      </mdController>
      <mdController name="dss.vector.solutions.PropertyController">
        <mdAction name="setFlag" isPost="true">
          <create>
            <mdParameter name="upfile" label="File" order="0" type="com.runwaysdk.controller.MultipartFileParameter" />
          </create>
        </mdAction>
      </mdController>

    </update>
  </doIt>
  <undoIt>
    <delete></delete>
  </undoIt>
