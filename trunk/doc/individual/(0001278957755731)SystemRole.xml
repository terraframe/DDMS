<version
  xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <delete>
      <object
        type="com.runwaysdk.system.Roles"
        key="Roles.mdss.coordinator" />
      <object
        type="com.runwaysdk.system.Roles"
        key="Roles.mdss.entomologist" />
      <object
        type="com.runwaysdk.system.Roles"
        key="Roles.mdss.operationalManager" />
      <object
        type="com.runwaysdk.system.Roles"
        key="Roles.mdss.MDSS" />
      <object
        type="com.runwaysdk.system.Roles"
        key="Roles.mdss.dataCapturer" />
      <object
        type="com.runwaysdk.system.Roles"
        key="Roles.mdss.manager" />
    </delete>
    <create>
      <object
        type="com.runwaysdk.system.Roles"
        key="Roles.mdss.System">
        <attribute
          name="roleName"
          value="mdss.System" />
        <attributeStruct
          name="displayLabel">
          <attribute
            name="defaultLocale"
            value="System" />
        </attributeStruct>
      </object>

    </create>
    <permissions>
      <role
        roleName="mdss.System">
        <superRole
          roleName="Administrator" />
      </role>
    </permissions>
  </doIt>
  <undoIt>
    <delete></delete>
  </undoIt>
</version>
