<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <update>
      <mdView
        name="dss.vector.solutions.export.IndividualCaseExcelView"
        label="Individual Case Excel View"
        description="Excel view for individual cases"
        publish="false">
        <attributes>
          <char
            name="diagnosisType"
            label="Type of Diagnosis"
            size="255"
            required="true"/>
        </attributes>
      </mdView>
    </update>
    <create>
      <mdProblem
        name="dss.vector.solutions.export.InvalidDiagnosisTypeProblem"
        label="Invalid Diagnosis Type Problem"
        description="Thrown if the user supplies a diagnosis type that is not recognized"
        message="[{diagnosisType}] is not a valid diagnosis type.">
        <attributes>
          <char
            name="diagnosisType"
            label="Type of Diagnosis"
            size="255"/>
        </attributes>
      </mdProblem>
    </create>
  </doIt>
  <undoIt>
    <delete>
      <object
        key="dss.vector.solutions.export.InvalidDiagnosisTypeProblem"
        type="com.runwaysdk.system.metadata.MdProblem"/>
    </delete>
  </undoIt>
</version>
