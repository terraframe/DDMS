<version
  xsi:noNamespaceSchemaLocation="../profiles/version_gis.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdException
        name="dss.vector.solutions.geo.PoliticalHierarchyLengthException"
        label="Political Hierarchy Length Exception"
        description="The political hierarchy is longer than the number of allocated spots for the geo entity attribute.">
        <attributes>
          <integer
            name="slots"
            label="Slots"
            required="true"
            description="The number of allocated spots for a geo entity attribute" />
          <integer
            name="hierarchyLength"
            label="Hierarchy Length"
            required="true"
            description="Length of the political hierarchy" />
        </attributes>
      </mdException>
    </create>
    <update>
      <mdBusiness
        name="dss.vector.solutions.entomology.MosquitoCollection">
        <create>
          <mdMethod
            name="getAdultDoseAssays"
            label="Get Adult Dose Assay"
            static="false"
            returnType="dss.vector.solutions.entomology.assay.AdultDiscriminatingDoseAssayQuery"
            description="Returns a query of all the Adult Discriminating Dose Assay which belong to this Mosquito Collection">
            <mdParameter
              name="sortAttribute"
              label="Sort Attribute"
              type="java.lang.String"
              order="0" />
            <mdParameter
              name="isAscending"
              label="Is Ascending"
              type="java.lang.Boolean"
              order="1" />
            <mdParameter
              name="pageSize"
              label="Page Size"
              type="java.lang.Integer"
              order="2" />
            <mdParameter
              name="pageNumber"
              label="Page Number"
              type="java.lang.Integer"
              order="3" />
          </mdMethod>
          <mdMethod
            name="getLarvaeDoseAssays"
            label="Get Adult Dose Assay"
            static="false"
            returnType="dss.vector.solutions.entomology.assay.LarvaeDiscriminatingDoseAssayQuery"
            description="Returns a query of all the Larvae Discriminating Dose Assay which belong to this Mosquito Collection">
            <mdParameter
              name="sortAttribute"
              label="Sort Attribute"
              type="java.lang.String"
              order="0" />
            <mdParameter
              name="isAscending"
              label="Is Ascending"
              type="java.lang.Boolean"
              order="1" />
            <mdParameter
              name="pageSize"
              label="Page Size"
              type="java.lang.Integer"
              order="2" />
            <mdParameter
              name="pageNumber"
              label="Page Number"
              type="java.lang.Integer"
              order="3" />
          </mdMethod>
          <mdMethod
            name="getKnockDownAssays"
            label="Get Adult Dose Assay"
            static="false"
            returnType="dss.vector.solutions.entomology.assay.KnockDownAssayQuery"
            description="Returns a query of all the Knock Down Assay which belong to this Mosquito Collection">
            <mdParameter
              name="sortAttribute"
              label="Sort Attribute"
              type="java.lang.String"
              order="0" />
            <mdParameter
              name="isAscending"
              label="Is Ascending"
              type="java.lang.Boolean"
              order="1" />
            <mdParameter
              name="pageSize"
              label="Page Size"
              type="java.lang.Integer"
              order="2" />
            <mdParameter
              name="pageNumber"
              label="Page Number"
              type="java.lang.Integer"
              order="3" />
          </mdMethod>
        </create>
      </mdBusiness>
      <mdController
        name="dss.vector.solutions.entomology.MosquitoCollectionController">
        <create>
          <mdAction
            name="viewADAPage"
            label="View Adult Dose Assay Page"
            description="View Page of Adult Dose Assay which belong to a collection"
            isPost="false"
            isQuery="true">
            <mdParameter
              name="collectionId"
              label="Colleciton Id"
              type="java.lang.String"
              order="0" />
          </mdAction>
          <mdAction
            name="viewLDAPage"
            label="View Larvae Dose Assay Page"
            description="View Page of Larvae Dose Assay which belong to a collection"
            isPost="false"
            isQuery="true">
            <mdParameter
              name="collectionId"
              label="Colleciton Id"
              type="java.lang.String"
              order="0" />
          </mdAction>
          <mdAction
            name="viewKDAPage"
            label="View knock down Assay Page"
            description="View Page of knock down Assay which belong to a collection"
            isPost="false"
            isQuery="true">
            <mdParameter
              name="collectionId"
              label="Colleciton Id"
              type="java.lang.String"
              order="0" />
          </mdAction>
        </create>
      </mdController>
    </update>
  </doIt>
  <undoIt>
    <delete></delete>
  </undoIt>
</version>