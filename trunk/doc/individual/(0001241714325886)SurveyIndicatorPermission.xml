<version xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <permissions>
      <role roleName="mdss.GUIVisibility">
        <grant>
          <mdViewPermission type="dss.vector.solutions.geo.GeoHierarchy">
            <operation name="READ" />
            <operation name="READ_ALL_ATTRIBUTES" />
          </mdViewPermission>

          <mdViewPermission type="dss.vector.solutions.geo.GeoEntityView">
            <operation name="CREATE" />
            <operation name="READ" />
            <operation name="READ_ALL_ATTRIBUTES" />
            <operation name="WRITE" />
            <operation name="WRITE_ALL_ATTRIBUTES" />
          </mdViewPermission>

          <mdBusinessPermission
            type="dss.vector.solutions.geo.generated.GeoEntity">
            <operation name="READ" />
            <operation name="READ_ALL_ATTRIBUTES" />
          </mdBusinessPermission>

          <mdRelationshipPermission type="dss.vector.solutions.geo.LocatedIn">
            <operation name="READ" />
            <operation name="READ_ALL_ATTRIBUTES" />
            <operation name="READ_CHILD" />
            <operation name="READ_PARENT" />
          </mdRelationshipPermission>

          <mdBusinessPermission type="com.terraframe.mojo.system.Users">
            <operation name="READ" />
            <attributePermission attributeName="username">
              <operation name="READ" />
            </attributePermission>
            <attributePermission attributeName="locale">
              <operation name="READ" />
            </attributePermission>
          </mdBusinessPermission>

          <mdBusinessPermission
            type="dss.vector.solutions.intervention.monitor.Net">
            <operation name="READ" />
            <operation name="READ_ALL_ATTRIBUTES" />
          </mdBusinessPermission>
          <mdRelationshipPermission
            type="dss.vector.solutions.intervention.monitor.NetHeiarchy">
            <operation name="READ" />
            <operation name="READ_PARENT" />
            <operation name="READ_CHILD" />
            <operation name="READ_ALL_ATTRIBUTES" />
          </mdRelationshipPermission>
          <mdBusinessPermission
            type="dss.vector.solutions.intervention.monitor.WindowMaster">
            <operation name="READ" />
            <operation name="READ_ALL_ATTRIBUTES" />
          </mdBusinessPermission>
          <mdBusinessPermission
            type="dss.vector.solutions.intervention.monitor.Household">
            <operation name="READ" />
            <operation name="READ_ALL_ATTRIBUTES" />
          </mdBusinessPermission>
          <mdRelationshipPermission
            type="dss.vector.solutions.intervention.monitor.SurveyHousehold">
            <operation name="READ" />
            <operation name="READ_PARENT" />
            <operation name="READ_CHILD" />
            <operation name="READ_ALL_ATTRIBUTES" />
          </mdRelationshipPermission>
          <mdRelationshipPermission
            type="dss.vector.solutions.intervention.monitor.HouseholdNet">
            <operation name="READ" />
            <operation name="READ_PARENT" />
            <operation name="READ_CHILD" />
            <operation name="READ_ALL_ATTRIBUTES" />
          </mdRelationshipPermission>
          <mdBusinessPermission
            type="dss.vector.solutions.entomology.SexMaster">
            <operation name="READ" />
            <operation name="READ_ALL_ATTRIBUTES" />
          </mdBusinessPermission>
          <mdBusinessPermission
            type="dss.vector.solutions.intervention.ResponseMaster">
            <operation name="READ" />
            <operation name="READ_ALL_ATTRIBUTES" />
          </mdBusinessPermission>
          <mdBusinessPermission
            type="dss.vector.solutions.intervention.RDTResultMaster">
            <operation name="READ" />
            <operation name="READ_ALL_ATTRIBUTES" />
          </mdBusinessPermission>
          <mdRelationshipPermission
            type="dss.vector.solutions.intervention.monitor.HouseholdPerson">
            <operation name="READ" />
            <operation name="READ_PARENT" />
            <operation name="READ_CHILD" />
            <operation name="READ_ALL_ATTRIBUTES" />
          </mdRelationshipPermission>
          <mdViewPermission
            type="dss.vector.solutions.intervention.monitor.PersonView">
            <operation name="READ" />
            <operation name="READ_ALL_ATTRIBUTES" />
            <operation name="CREATE" />
            <operation name="WRITE_ALL_ATTRIBUTES" />
            <operation name="WRITE_ALL_ATTRIBUTES" />
          </mdViewPermission>
        </grant>
      </role>
      <role roleName="mdss.dataCapturer">
        <grant>
          <mdBusinessPermission
            type="dss.vector.solutions.intervention.monitor.SurveyPoint">
            <operation name="CREATE" />
            <operation name="WRITE" />
            <operation name="WRITE_ALL_ATTRIBUTES" />
            <operation name="DELETE" />
          </mdBusinessPermission>
          <mdBusinessPermission
            type="dss.vector.solutions.intervention.monitor.Household">
            <operation name="CREATE" />
            <operation name="WRITE" />
            <operation name="WRITE_ALL_ATTRIBUTES" />
            <operation name="DELETE" />
          </mdBusinessPermission>
          <mdRelationshipPermission
            type="dss.vector.solutions.intervention.monitor.SurveyHousehold">
            <operation name="CREATE" />
            <operation name="WRITE" />
            <operation name="WRITE_ALL_ATTRIBUTES" />
            <operation name="DELETE" />
          </mdRelationshipPermission>
          <mdRelationshipPermission
            type="dss.vector.solutions.intervention.monitor.HouseholdNet">
            <operation name="CREATE" />
            <operation name="WRITE" />
            <operation name="WRITE_ALL_ATTRIBUTES" />
            <operation name="DELETE" />
          </mdRelationshipPermission>
          <mdBusinessPermission
            type="dss.vector.solutions.intervention.monitor.Person">
            <operation name="CREATE" />
            <operation name="WRITE" />
            <operation name="WRITE_ALL_ATTRIBUTES" />
            <operation name="DELETE" />
          </mdBusinessPermission>
          <mdRelationshipPermission
            type="dss.vector.solutions.intervention.monitor.HouseholdPerson">
            <operation name="CREATE" />
            <operation name="WRITE" />
            <operation name="WRITE_ALL_ATTRIBUTES" />
            <operation name="DELETE" />
          </mdRelationshipPermission>
        </grant>
      </role>
    </permissions>
  </doIt>
  <undoIt>
    <!-- There is no way to revoke permissions through xml at the moment -->
  </undoIt>
</version>