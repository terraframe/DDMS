<version
	xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<doIt>
		<create>
			<object
				type="com.runwaysdk.system.metadata.MdDimension"
				key="com.runwaysdk.system.metadata.MdDimension.MALARIA">
				<attribute
					name="name"
					value="MALARIA" />
				<attributeStruct
					name="displayLabel">
					<attribute
						name="defaultLocale"
						value="Malaria" />
				</attributeStruct>
			</object>
			<object
				type="com.runwaysdk.system.metadata.MdDimension"
				key="com.runwaysdk.system.metadata.MdDimension.DENGUE">
				<attribute
					name="name"
					value="DENGUE" />
				<attributeStruct
					name="displayLabel">
					<attribute
						name="defaultLocale"
						value="Dengue" />
				</attributeStruct>
			</object>


			<mdBusiness
				name="dss.vector.solutions.general.Disease"
				label="Disease"
				generateController="false"
				cacheAlgorithm="everything">
				<attributes>
					<reference
						name="dimension"
						label="Dimension"
						type="com.runwaysdk.system.metadata.MdDimension"
						required="true" />
					<reference
						name="menuRoot"
						label="Menu Root"
						required="false"
						description="A menu term"
						type="dss.vector.solutions.ontology.Term" />
				</attributes>
				<mdMethod
					name="getMenuJson"
					label="Get Menu JSON"
					description="Returns the JSON to create the menu"
					static="true"
					returnType="java.lang.String">
				</mdMethod>
			</mdBusiness>

			<object
				key="MALARIA"
				type="dss.vector.solutions.general.Disease">
				<attributeReference
					name="dimension"
					key="com.runwaysdk.system.metadata.MdDimension.MALARIA" />
			</object>

			<object
				key="DENGUE"
				type="dss.vector.solutions.general.Disease">
				<attributeReference
					name="dimension"
					key="com.runwaysdk.system.metadata.MdDimension.DENGUE" />
			</object>
		</create>

		<update>
			<mdBusiness
				name="dss.vector.solutions.UserSettings">
				<create>
					<attributes>
						<reference
							name="disease"
							label="Disease"
							required="true"
							type="dss.vector.solutions.general.Disease"
							defaultValueKey="MALARIA" />
					</attributes>
				</create>
			</mdBusiness>
		</update>
		<update>
			<mdView
				name="dss.vector.solutions.PersonView">
				<create>
					<attributes>
						<virtual
							concrete="disease"
							type="dss.vector.solutions.UserSettings" />
					</attributes>
				</create>
			</mdView>
			<mdView
				name="dss.vector.solutions.export.PersonExcelView">
				<create>
					<attributes>
						<char
							name="disease"
							label="Current Disease"
							size="64"
							type="dss.vector.solutions.UserSettings" />
					</attributes>
				</create>
			</mdView>
		</update>
	</doIt>
	<undoIt>
		<delete>
			<object
				key="dss.vector.solutions.general.Disease"
				type="com.runwaysdk.system.metadata.MdBusiness" />
			<object
				key="com.runwaysdk.system.metadata.MdDimension.MALARIA"
				type="com.runwaysdk.system.metadata.MdDimension" />
			<object
				key="com.runwaysdk.system.metadata.MdDimension.DENGUE"
				type="com.runwaysdk.system.metadata.MdDimension" />
			<object
				key="dss.vector.solutions.PersonView.disease"
				type="com.runwaysdk.system.metadata.MdAttributeVirtual" />
			<object
				key="dss.vector.solutions.export.PersonExcelView.disease"
				type="com.runwaysdk.system.metadata.MdAttributeCharacter" />
			<object
				key="dss.vector.solutions.UserSettings.disease"
				type="com.runwaysdk.system.metadata.MdAttributeEnumeration" />
		</delete>
	</undoIt>
</version>