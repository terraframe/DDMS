<version
  xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <update>
      <mdView
        name="dss.vector.solutions.export.GeoEntityExcelView">
        <create>
          <attributes>
            <char
              name="parentName"
              label="Located In Name/ID"
              description="Parent geo entity name or geo id"
              size="255"
              required="false" />
            <char
              name="parentType"
              label="Located In Type"
              description="Parent geo entity name or geo id"
              size="255"
              required="false" />
          </attributes>
        </create>
      </mdView>
      <mdBusiness
        name="dss.vector.solutions.util.Facade">
        <create>
          <mdMethod
            name="exportGeoChildren"
            label="Export Geo Children"
            description="Exports all geos located in the given entity"
            returnType="java.io.InputStream"
            static="true">
            <mdParameter
              name="parentId"
              label="Geo Parent ID"
              order="0"
              type="java.lang.String" />
            <mdParameter
              name="includeGeoData"
              label="Include Geo Data"
              order="1"
              type="java.lang.Boolean" />
          </mdMethod>
          <mdMethod
            name="exportGeosByType"
            label="Export Geos by Type"
            description="Exports all geos of a given type"
            returnType="java.io.InputStream"
            static="true">
            <mdParameter
              name="hierarchyId"
              label="Hierarchy ID"
              order="0"
              type="java.lang.String" />
            <mdParameter
              name="includeGeoData"
              label="Include Geo Data"
              order="1"
              type="java.lang.Boolean" />
          </mdMethod>
        </create>
      </mdBusiness>
      <mdController
        name="dss.vector.solutions.geo.GeoEntityTypeController">
        <create>
          <mdAction
            name="export"
            isPost="false"
            label="Export Geo Entities">
            <mdParameter
              name="includeGeoData"
              label="Include Geo Data"
              order="0"
              type="java.lang.Boolean" />
          </mdAction>
        </create>
      </mdController>
      <mdController
        name="dss.vector.solutions.geo.GeoEntityTreeController">
        <create>
          <mdAction
            name="export"
            isPost="false"
            label="Export Geo Entities">
            <mdParameter
              name="includeGeoData"
              label="Include Geo Data"
              order="0"
              type="java.lang.Boolean" />
          </mdAction>
        </create>
      </mdController>
    </update>
  </doIt>
  <undoIt>
    <delete></delete>
  </undoIt>
</version>