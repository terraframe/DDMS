<version xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <enumerationMaster
        name="dss.vector.solutions.general.DiseaseMaster"
        label="Disease Master"
        generateController="false">
      </enumerationMaster>
      
      <object 
        type="dss.vector.solutions.general.DiseaseMaster"
        key="dss.vector.solutions.general.DiseaseMaster.Malaria">
        <attribute
          name="enumName"
          value="MALARIA"/>
        <attributeStruct
          name="displayLabel">
          <attribute
            name="defaultLocale"
            value="Malaria" />
        </attributeStruct>
      </object>
      
      <object 
        type="dss.vector.solutions.general.DiseaseMaster"
        key="dss.vector.solutions.general.DiseaseMaster.Dengue">
        <attribute
          name="enumName"
          value="DENGUE"/>
        <attributeStruct
          name="displayLabel">
          <attribute
            name="defaultLocale"
            value="Dengue" />
        </attributeStruct>
      </object>
      
      <mdEnumeration
        name="dss.vector.solutions.general.Disease"
        type="dss.vector.solutions.general.DiseaseMaster"
        label="Disease">
        <includeAll />
      </mdEnumeration>
    </create>
    
    <update>
       <mdBusiness
         name="dss.vector.solutions.UserSettings">
         <create>
           <attributes>
             <enumeration
               name="disease"
               label="Disease"
               required="true"
               type="dss.vector.solutions.general.Disease"
               selectMultiple="false"
               defaultValueKey="dss.vector.solutions.general.DiseaseMaster.MALARIA"/>
           </attributes>
         </create>
       </mdBusiness>
    </update>
    <update>
       <mdView
         name="dss.vector.solutions.PersonView">
         <create>
           <attributes>
             <virtual
               concrete="disease"
               type="dss.vector.solutions.UserSettings"/>
           </attributes>
         </create>
       </mdView>
       <mdView
         name="dss.vector.solutions.export.PersonExcelView">
         <create>
           <attributes>
             <char
               name="disease"
               label="Current Disease"
               size="64"
               type="dss.vector.solutions.UserSettings"/>
           </attributes>
         </create>
       </mdView>
    </update>
  </doIt>
  <undoIt>
    <delete>
      <object
        key="dss.vector.solutions.PersonView.disease"
        type="com.runwaysdk.system.metadata.MdAttributeVirtual" />
      <object
        key="dss.vector.solutions.export.PersonExcelView.disease"
        type="com.runwaysdk.system.metadata.MdAttributeCharacter" />
      <object
        key="dss.vector.solutions.UserSettings.disease"
        type="com.runwaysdk.system.metadata.MdAttributeEnumeration" />
      <object
        key="dss.vector.solutions.general.Disease"
        type="com.runwaysdk.system.metadata.MdEnumeration" />
      <object
        key="dss.vector.solutions.general.DiseaseMaster"
        type="com.runwaysdk.system.metadata.MdBusiness" />
    </delete>
  </undoIt>
</version>