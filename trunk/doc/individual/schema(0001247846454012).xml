        description="Mixed assay"
        generateController="false">
        <attributes>
          <boolean
            name="testResult"
            label="Mixed"
            required="false"
            description="assay result"
            positiveLabel="Positive"
            negativeLabel="Negative" />
        </attributes>
      </mdBusiness>
    </create>
    <update>
      <mdBusiness
        name="dss.vector.solutions.intervention.monitor.Net">
        <create>
          <mdMethod
            name="getPage"
            label="getPage"
            static="true"
            returnType="com.terraframe.mojo.query.ValueQuery">
            <mdParameter
              name="sortAttribute"
              label="Sort Attribute"
              type="java.lang.String"
              order="0" />
            <mdParameter
              name="isAscending"
              label="Is Ascending"
              type="java.lang.Boolean"
              order="1" />
            <mdParameter
              name="pageSize"
              label="Page Size"
              type="java.lang.Integer"
              order="2" />
            <mdParameter
              name="pageNumber"
              label="Page Number"
              type="java.lang.Integer"
              order="3" />
          </mdMethod>
        </create>
      </mdBusiness>
      <mdView
        name="dss.vector.solutions.export.MosquitoExcelView">
        <create>
          <attributes>
            <char
              name="mixed"
              label="Mixed"
              size="255" />
            <char
              name="mixedMethod"
              label="Mixed Method"
              size="255" />
          </attributes>
        </create>
      </mdView>
      <mdView
        name="dss.vector.solutions.entomology.MosquitoView">
        <create>
          <attributes>
            <virtual
              name="mixed"
              type="dss.vector.solutions.entomology.assay.infectivity.MixedTestResult"
              concrete="testResult"
              required="false" />
            <virtual
              name="mixedMethod"
              type="dss.vector.solutions.entomology.assay.infectivity.InfectivityAssayTestResult"
              label="Mixed Method"
              concrete="testMethod"
              required="false" />
          </attributes>
        </create>
      </mdView>
    </update>
  </doIt>
  <undoIt>
    <delete></delete>
  </undoIt>
