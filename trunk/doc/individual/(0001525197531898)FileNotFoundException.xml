<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdException name="dss.vector.solutions.FileNotFoundException" message="The file [{name}] at location [{path}] was not found">
        <attributes>
		  <text name="name" label="Name"></text>
		  <text name="path" label="Path"></text>
		</attributes>        
