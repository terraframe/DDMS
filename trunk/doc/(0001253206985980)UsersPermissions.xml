<version
  xsi:noNamespaceSchemaLocation="../profiles/version_gis.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <permissions>
      <role
        roleName="mdss.coordinator">
        <grant>
          <mdBusinessPermission
            type="com.terraframe.mojo.system.Users">
            <operation
              name="WRITE" />
            <attributePermission
              attributeName="username">
              <operation
                name="WRITE" />
            </attributePermission>
            <attributePermission
              attributeName="password">
              <operation
                name="WRITE" />
            </attributePermission>
            <attributePermission
              attributeName="sessionLimit">
              <operation
                name="WRITE" />
            </attributePermission>
          </mdBusinessPermission>
        </grant>
      </role>
    </permissions>
  </doIt>
  <undoIt>
  </undoIt>
</version>