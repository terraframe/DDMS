<version
  xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdException
        name="dss.vector.solutions.irs.NotASprayEntityException"
        label="Not a Spray Entity Exception">
        <attributes>
          <char
            name="entityLabel"
            label="Geo Entity"
            size="255" />
        </attributes>
      </mdException>
    </create>
    <update>
      <mdView
        name="dss.vector.solutions.irs.GeoTargetView">
        <create>
          <mdMethod
            name="getGeoTargetViews"
            label="Get Geo Target Views"
            returnType="dss.vector.solutions.irs.GeoTargetView[]"
            static="true">
            <mdParameter
              name="geoId"
              label="Geo Id"
              order="0"
              type="java.lang.String" />
            <mdParameter
              name="season"
              label="Season"
              order="1"
              type="dss.vector.solutions.general.MalariaSeason" />
          </mdMethod>
        </create>
      </mdView>
      <mdView
        name="dss.vector.solutions.general.PopulationDataView">
        <create>
          <mdMethod
            name="getCalculatedPopulation"
            label="Get Calculated Population"
            returnType="java.lang.Long"
            static="false" />
        </create>
      </mdView>
    </update>
  </doIt>
  <undoIt>
    <delete>
      <object
        key="dss.vector.solutions.irs.NotASprayEntityException"
        type="com.runwaysdk.system.metadata.MdException" />
      <object
        key="dss.vector.solutions.irs.GeoTargetView.getGeoTargetViews"
        type="com.runwaysdk.system.metadata.MdMethod" />
      <object
        key="dss.vector.solutions.general.PopulationDataView.getCalculatedPopulation"
        type="com.runwaysdk.system.metadata.MdMethod" />
    </delete>
  </undoIt>
</version>
