  xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdException
        name="dss.vector.solutions.UnknownGeoEntityException"
        label="Unknown Geo Entity Exception" />
      <mdException
        name="dss.vector.solutions.AmbigiousGeoEntityException"
        label="Ambigious Geo Entity Exception" />
