<version
  xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdView
        name="dss.vector.solutions.query.QueryBuilder">
        <mdMethod
          name="getQueryResults"
          label="Get Query Results"
          description="Run the Query on the passed method"
          static="true"
          returnType="com.runwaysdk.query.ValueQuery">
          <mdParameter
            name="queryType"
            label="Query Type"
            description="The FQCN this method will be run on"
            order="0"
            type="java.lang.String"/>
          <mdParameter
            name="queryXML"
            label="Query XML"
            description="Query XML"
            order="1"
            type="java.lang.String"/>
          <mdParameter
            name="config"
            label="Config"
            description="The type on which to gather spatial data"
            order="2"
            type="java.lang.String"/>
          <mdParameter
            name="sortBy"
            label="Sort By"
            type="java.lang.String"
            order="3"/>
          <mdParameter
            name="ascending"
            label="Ascending"
            type="java.lang.Boolean"
            order="4"/>
          <mdParameter
            name="pageNumber"
            label="Page Number"
            type="java.lang.Integer"
            order="5"/>
          <mdParameter
            name="pageSize"
            label="Page Size"
            type="java.lang.Integer"
            order="6"/>
        </mdMethod>
        <mdMethod
          name="mapQuery"
          label="Map Query"
          description="Map Query"
          static="true"
          returnType="java.lang.String">
          <mdParameter
            name="queryType"
            label="Query Type"
            description="The FQCN this method will be run on"
            order="0"
            type="java.lang.String"/>
          <mdParameter
            name="queryXML"
            label="Query XML"
            description="Query XML"
            order="1"
            type="java.lang.String"/>
          <mdParameter
            name="thematicLayerType"
            label="Thematic Layer Type"
            description="The GeoEntity type for the base layer with thematic variables."
            order="2"
            type="java.lang.String"/>
          <mdParameter
            name="universalLayers"
            label="Universal Layers"
            description="The universal types that will act as additional layers"
            order="3"
            type="java.lang.String[]"/>
          <mdParameter
            name="savedSearchId"
            label="SavedSearch Id"
            description="The id of the SavedSearch for which the map will be generated."
            order="4"
            type="java.lang.String"/>
        </mdMethod>
        <mdMethod
          name="exportQueryToExcel"
          label="Export Query To Excel"
          static="true"
          returnType="java.io.InputStream">
          <mdParameter
            name="queryType"
            label="Query Type"
            description="The FQCN this method will be run on"
            order="0"
            type="java.lang.String"/>
          <mdParameter
            name="queryXML"
            label="Query XML"
            description="Query XML"
            order="1"
            type="java.lang.String"/>
          <mdParameter
            name="config"
            label="Config"
            description="The type on which to gather spatial data"
            order="2"
            type="java.lang.String"/>
          <mdParameter
            name="savedSearchId"
            label="Saved Search Id"
            order="3"
            type="java.lang.String"/>
        </mdMethod>
        <mdMethod
          name="exportQueryToCSV"
          label="Export Query To CSV"
          static="true"
          returnType="java.io.InputStream">
          <mdParameter
            name="queryType"
            label="Query Type"
            description="The FQCN this method will be run on"
            order="0"
            type="java.lang.String"/>
          <mdParameter
            name="queryXML"
            label="Query XML"
            description="Query XML"
            order="1"
            type="java.lang.String"/>
          <mdParameter
            name="config"
            label="Config"
            description="The type on which to gather spatial data"
            order="2"
            type="java.lang.String"/>
          <mdParameter
            name="savedSearchId"
            label="Saved Search Id"
            order="3"
            type="java.lang.String"/>
        </mdMethod>
      </mdView>

    </create>
    <update></update>
  </doIt>
  <undoIt>
    <delete></delete>
  </undoIt>
</version>
