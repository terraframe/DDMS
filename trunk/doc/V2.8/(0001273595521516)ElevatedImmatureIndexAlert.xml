<version
	xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<doIt>
		<create>
			<mdInformation
				name="dss.vector.solutions.general.ElevatedImmatureIndexAlert"
				label="Elevated Immature Index Alert">
				<attributes>
					<char
						name="alertType"
						label="Alert Type"
						size="255" />
					<char
						name="thresholdType"
						label="Threshold Type"
						size="255" />
					<decimal
						name="thresholdValue"
						label="Threshold Value"
						length="32"
						decimal="2"
						required="true" />
					<decimal
						name="actualValue"
						label="Actual Value"
						length="32"
						decimal="2"
						required="true" />
					<char
						name="geoEntity"
						label="GeoEntity Label"
						size="255" />
					<char
						name="premiseType"
						label="Premise Type"
						size="255" />
					<char
						name="taxon"
						label="Taxon"
						size="255" />
					<date
						name="startDate"
						label="Start Date" />
					<date
						name="endDate"
						label="End Date" />
					<boolean
						name="emailFailure"
						label="Email not sent"
						defaultValue="false"
						positiveLabel="This alert was configured to send an email; however, sending the email was unsuccessful at this time.  It will be retried according to the current configuration settings."
						negativeLabel="" />
				</attributes>
			</mdInformation>
		</create>
	</doIt>
	<undoIt>
		<delete></delete>
	</undoIt>
</version>
