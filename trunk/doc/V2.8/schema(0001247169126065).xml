        <attributeStruct
          name="displayLabel">
          <attribute
            name="defaultLocale"
            value="IRS" />
        </attributeStruct>
      </object>
      <object
        type="dss.vector.solutions.QueryTypeMaster"
        key="dss.vector.solutions.QueryTypeMaster.ENTOMOLOGY">
        <attribute
          name="enumName"
          value="ENTOMOLOGY" />
        <attributeStruct
          name="displayLabel">
          <attribute
            name="defaultLocale"
            value="Entomology" />
        </attributeStruct>
      </object>
      <object
        type="dss.vector.solutions.QueryTypeMaster"
        key="dss.vector.solutions.QueryTypeMaster.AGGREGATED_CASES">
        <attribute
          name="enumName"
          value="AGGREGATED_CASES" />
        <attributeStruct
          name="displayLabel">
          <attribute
            name="defaultLocale"
            value="Aggregated Cases" />
