<version
	xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<doIt>
		<create>
			<mdProblem
				name="dss.vector.solutions.WKTParsingProblem"
				label="WKT Parsing Problem"
				description="Raised when there is an error parsing WKT for a GeoEntity."
				extends="dss.vector.solutions.NotificationProblem">
				<attributes>
					<char
						name="reason"
						label="Reason"
						size="255" />
				</attributes>
			</mdProblem>
		</create>
	</doIt>
	<undoIt>
		<delete>
			<object
				key="dss.vector.solutions.WKTParsingProblem"
				type="com.runwaysdk.system.metadata.MdProblem" />
		</delete>
	</undoIt>
</version>
