<version xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
    
      <mdException
        name="dss.vector.solutions.query.ImageUploadOnlyException"
        label="Imge Upload Only Exception"
        description="Thrown when a user tries to upload something other than image to place on a map.">
      </mdException>
    
    </create>
    <update>
    
      <mdController name="dss.vector.solutions.query.MappingController">
        <create>
          <mdAction
            name="uploadMapImage"
            label="Upload Map Image"
            isPost="true">
          </mdAction>
        </create>
      </mdController>    
    
    </update>
  </doIt>
  <undoIt>
    <delete>
    
      <object
        key="dss.vector.solutions.query.MappingController.uploadMapImage"
        type="com.runwaysdk.system.metadata.MdAction" />
    
      <object
        key="dss.vector.solutions.query.ImageUploadOnlyException"
        type="com.runwaysdk.system.metadata.MdException"/>
    
    </delete>
  </undoIt>
</version>
