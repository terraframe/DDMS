<version
  xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <delete>
      <object
        key="dss.vector.solutions.export.ThresholdDataExcelView.startDate"
        type="com.runwaysdk.system.metadata.MdAttributeVirtual"/>
      <object
        key="dss.vector.solutions.export.ThresholdDataExcelView.endDate"
        type="com.runwaysdk.system.metadata.MdAttributeVirtual"/>
    </delete>
    <update>
      <mdView
        name="dss.vector.solutions.export.ThresholdDataExcelView"
        label="Threshold Data Excel View">
        <create>
          <attributes>
            <virtual
              concrete="thresholdType"
              label="Threshold Type"
              description="Political or Facility"
              type="dss.vector.solutions.general.ThresholdDataView" />
          </attributes>
        </create>
      </mdView>
    </update>
  </doIt>
  <undoIt>
    <update>
      <mdView
        name="dss.vector.solutions.export.ThresholdDataExcelView"
        label="Threshold Data Excel View">
        <create>
          <attributes>
            <virtual
              concrete="startDate"
              type="dss.vector.solutions.general.MalariaSeason" />
            <virtual
              concrete="endDate"
              type="dss.vector.solutions.general.MalariaSeason" />
          </attributes>
        </create>
      </mdView>
    </update>
    <delete>
      <object
        key="dss.vector.solutions.export.ThresholdDataExcelView.thresholdType"
        type="com.runwaysdk.system.metadata.MdAttributeVirtual"/>
    </delete>
  </undoIt>
</version>
