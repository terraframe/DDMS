<version
  xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <update>
      <mdView
        name="dss.vector.solutions.util.ReadableAttributeView">
        <create>
          <attributes>
            <char
              name="fieldId"
              label="Browser Field Id"
              required="false"
              size="64" />
          </attributes>
        </create>
      </mdView>
      <mdBusiness
        name="dss.vector.solutions.ontology.BrowserField">
        <create>
          <mdMethod
            name="getView"
            label="Get View"
            returnType="dss.vector.solutions.ontology.BrowserFieldView"
            static="false" />
        </create>
      </mdBusiness>
      <mdView
        name="dss.vector.solutions.ontology.BrowserFieldView">
        <create>
          <mdMethod
            name="getRoots"
            returnType="dss.vector.solutions.ontology.BrowserRootView[]"
            label="Get Roots"
            static="false" />
        </create>
      </mdView>
    </update>
  </doIt>
  <undoIt>
    <delete>
      <object
        key="dss.vector.solutions.ontology.BrowserField.getView"
        type="com.runwaysdk.system.metadata.MdMethod" />
      <object
        key="dss.vector.solutions.util.ReadableAttributeView.fieldId"
        type="com.runwaysdk.system.metadata.MdAttribute" />
    </delete>
  </undoIt>
</version>
