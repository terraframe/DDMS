<version
	xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<doIt>
		<update>
			<mdBusiness
				name="dss.vector.solutions.general.SystemAlert">
				<create>
					<mdMethod
						name="getAllInstancesForDisease"
						label="Get All InstancedFor Disease"
						description="Returns all objects for the current disease"
						static="true"
						returnType="dss.vector.solutions.general.SystemAlertQuery">
						<mdParameter
							name="sortAttribute"
							label="Sort Attribute"
							order="0"
							type="java.lang.String" />
						<mdParameter
							name="ascending"
							label="Ascending"
							description="Whether to sort ascending"
							order="1"
							type="java.lang.Boolean" />
						<mdParameter
							name="pageSize"
							label="Page Size"
							type="java.lang.Integer"
							order="2" />
						<mdParameter
							name="pageNumber"
							label="Page Number"
							type="java.lang.Integer"
							order="3" />
					</mdMethod>
				</create>
			</mdBusiness>
		</update>
	</doIt>
	<undoIt>
		<delete></delete>
	</undoIt>
</version>
