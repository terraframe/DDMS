<version
  xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdView
        name="dss.vector.solutions.export.InfectionAssayExcelView"
        label="Infection Assay Excel View">
        <attributes>
          <virtual
            concrete="collectionId"
            type="dss.vector.solutions.entomology.MosquitoCollection"/>
          <virtual
            concrete="mosquitoId"
            type="dss.vector.solutions.entomology.InfectionAssay"/>
          <char
            name="species"
            label="Species"
            size="255"/>
          <char
            name="identMethod"
            label="Method of species identification"
            size="255"/>
          <char
            name="sex"
            label="Sex"
            size="255"/>
          <char
            name="parasite"
            label="Tested parasite"
            size="255"/>
          <char
            name="testMethod"
            label="Test Method"
            size="255"/>
          <virtual
            concrete="infected"
            type="dss.vector.solutions.entomology.InfectionAssay"/>
          <virtual
            concrete="numberTested"
            type="dss.vector.solutions.entomology.InfectionAssay"/>
          <virtual
            concrete="numberPositive"
            type="dss.vector.solutions.entomology.InfectionAssay"/>
        </attributes>
      </mdView>
      <mdView
        name="dss.vector.solutions.export.PooledInfectionAssayExcelView"
        label="Pooled Infection Assay Excel View">
        <attributes>
          <virtual
            concrete="collectionId"
            type="dss.vector.solutions.entomology.MosquitoCollection"/>
          <virtual
            concrete="poolId"
            type="dss.vector.solutions.entomology.PooledInfectionAssay"/>
          <char
            name="species"
            label="Species"
            size="255"/>
          <char
            name="identMethod"
            label="Method of species identification"
            size="255"/>
          <char
            name="sex"
            label="Sex"
            size="255"/>
          <char
            name="parasite"
            label="Tested parasite"
            size="255"/>
          <char
            name="testMethod"
            label="Test Method"
            size="255"/>
          <virtual
            concrete="infected"
            type="dss.vector.solutions.entomology.PooledInfectionAssay"/>
          <virtual
            concrete="mosquitosTested"
            type="dss.vector.solutions.entomology.PooledInfectionAssay"/>
          <virtual
            concrete="poolsTested"
            type="dss.vector.solutions.entomology.PooledInfectionAssay"/>
          <virtual
            concrete="numberPositive"
            type="dss.vector.solutions.entomology.PooledInfectionAssay"/>
        </attributes>
      </mdView>
    </create>
  </doIt>
  <undoIt>
    <delete>
      <object
        key="dss.vector.solutions.export.InfectionAssayExcelView"
        type="com.runwaysdk.system.metadata.MdView" />
      <object
        key="dss.vector.solutions.export.PooledInfectionAssayExcelView"
        type="com.runwaysdk.system.metadata.MdView" />
    </delete>
  </undoIt>
</version>
