<version
  xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdBusiness
        name="dss.vector.solutions.intervention.monitor.SurveyPoint"
        label="Survey Point"
        generateController="true">
        <attributes>
          <reference
            name="geoEntity"
            label="Geo Entity"
            description="The geoEntity of the survey"
            required="true"
            type="dss.vector.solutions.geo.generated.GeoEntity" />
          <date
            name="surveyDate"
            label="Date"
            required="true" />
        </attributes>
        <mdMethod
          name="searchByGeoEntityAndDate"
          label="Search By GeoEntity and Date"
          static="true"
          returnType="dss.vector.solutions.intervention.monitor.SurveyPoint">
          <mdParameter
            name="geoEntity"
            label="Geo Entity"
            order="0"
            type="dss.vector.solutions.geo.generated.GeoEntity" />
          <mdParameter
            name="date"
            label="Date"
            order="1"
            type="java.util.Date" />
        </mdMethod>
      </mdBusiness>

      <mdIndex
        type="dss.vector.solutions.intervention.monitor.SurveyPoint"
        active="true"
        unique="true"
        label="Survey Point Index">
        <attribute
          name="geoEntity"
          indexOrder="0" />
        <attribute
          name="surveyDate"
          indexOrder="1" />
      </mdIndex>

      <mdBusiness
        name="dss.vector.solutions.intervention.monitor.Household"
        label="Household"
        generateController="true"
        description="A surveyed household">
        <attributes>
          <reference
            name="surveyPoint"
            label="Survey Point"
            required="true"
            immutable="true"
            type="dss.vector.solutions.intervention.monitor.SurveyPoint" />
          <char
            name="householdName"
            label="Household Id"
            size="255"
            description="The name of the household"
            required="true"
            indexType="unique index" />
          <boolean
            name="urban"
            label="Household Type"
            description="Flag indicating if the household is urban or rural"
            negativeLabel="Urban"
            positiveLabel="Rural" />
          <integer
            name="people"
            label="Number of people living in household"
            description="The number of people living in the household"
            rejectNegative="true" />
          <reference
            name="wall"
            label="Wall Surface"
            description="The wall surface"
            type="dss.vector.solutions.ontology.Term" />
          <char
            name="wallInfo"
            label="Wall Information"
            size="255"
            description="The specific kind of wall if not in Wall Surface list" />
          <reference
            name="roof"
            label="Roof Surface"
            description="The roof surface"
            type="dss.vector.solutions.ontology.Term" />
          <char
            name="roofInfo"
            label="Roof Information"
            size="255"
            description="Extra information about the roof surface" />
          <boolean
            name="hasWindows"
            label="Windows"
            description="Does this house have any windows?"
            negativeLabel="No"
            positiveLabel="Yes" />
          <reference
            name="windowType"
            label="WindowType"
            type="dss.vector.solutions.ontology.Term" />
          <integer
            name="rooms"
            label="Number of rooms in household"
            rejectNegative="true" />
          <integer
            name="lastSprayed"
            label="Months since last sprayed"
            rejectNegative="true" />
          <integer
            name="nets"
            label="Number of nets in household"
            description="Number of nets in household"
            rejectNegative="true"
            defaultValue="0" />
          <integer
            name="sleptUnderNets"
            label="Number of people who slept under nets last night"
            description="How many people in this household  slept under a net last night?"
            rejectNegative="true"
            defaultValue="0" />
          <integer
            name="netsUsed"
            label="Number of nets used last night"
            description="The number of nets which were used to sleep under last night"
            rejectNegative="true"
            defaultValue="0" />
        </attributes>
        <mdMethod
          name="getView"
          label="Get View"
          returnType="dss.vector.solutions.intervention.monitor.HouseholdView"
          static="true">
          <mdParameter
            name="id"
            label="id"
            order="0"
            type="java.lang.String" />
        </mdMethod>
        <mdMethod
          name="lockView"
          label="lock View"
          returnType="dss.vector.solutions.intervention.monitor.HouseholdView"
          static="false" />
        <mdMethod
          name="unlockView"
          label="unlock View"
          returnType="dss.vector.solutions.intervention.monitor.HouseholdView"
          static="false" />
      </mdBusiness>

      <mdTree
        name="dss.vector.solutions.intervention.monitor.SurveyHousehold"
        label="Survey Household"
        generateController="false">
        <parent
          cardinality="1"
          name="dss.vector.solutions.intervention.monitor.SurveyPoint"
          method="SurveyPoints" />
        <child
          cardinality="*"
          name="dss.vector.solutions.intervention.monitor.Household"
          method="Households" />
      </mdTree>

      <mdTree
        name="dss.vector.solutions.intervention.monitor.HouseholdNet"
        label="Household Net"
        generateController="false">
        <attributes>
          <integer
            name="amount"
            label="amount"
            required="true"
            rejectNegative="true"
            defaultValue="0" />
        </attributes>
        <parent
          cardinality="*"
          name="dss.vector.solutions.intervention.monitor.Household"
          method="NetHouseholds" />
        <child
          cardinality="*"
          name="dss.vector.solutions.ontology.Term"
          method="Nets" />
      </mdTree>

      <mdView
        name="dss.vector.solutions.intervention.monitor.HouseholdView"
        label="Household">
        <attributes>
          <char
            name="concreteId"
            label="Concrete Id"
            size="255"
            required="true" />
          <reference
            name="displayNets"
            label="Number of nets against the brand"
            required="false"
            type="dss.vector.solutions.ontology.Term" />
          <virtual
            name="surveyPoint"
            concrete="surveyPoint"
            type="dss.vector.solutions.intervention.monitor.Household" />
          <virtual
            name="householdName"
            concrete="householdName"
            type="dss.vector.solutions.intervention.monitor.Household" />
          <virtual
            name="urban"
            concrete="urban"
            type="dss.vector.solutions.intervention.monitor.Household" />
          <virtual
            name="people"
            concrete="people"
            type="dss.vector.solutions.intervention.monitor.Household" />
          <virtual
            name="wall"
            concrete="wall"
            type="dss.vector.solutions.intervention.monitor.Household" />
          <virtual
            name="wallInfo"
            concrete="wallInfo"
            type="dss.vector.solutions.intervention.monitor.Household" />
          <virtual
            name="roof"
            concrete="roof"
            type="dss.vector.solutions.intervention.monitor.Household" />
          <virtual
            name="roofInfo"
            concrete="roofInfo"
            type="dss.vector.solutions.intervention.monitor.Household" />
          <virtual
            name="hasWindows"
            concrete="hasWindows"
            type="dss.vector.solutions.intervention.monitor.Household" />
          <virtual
            name="windowType"
            concrete="windowType"
            type="dss.vector.solutions.intervention.monitor.Household" />
          <virtual
            name="rooms"
            concrete="rooms"
            type="dss.vector.solutions.intervention.monitor.Household" />
          <virtual
            name="lastSprayed"
            concrete="lastSprayed"
            type="dss.vector.solutions.intervention.monitor.Household" />
          <virtual
            name="nets"
            concrete="nets"
            type="dss.vector.solutions.intervention.monitor.Household" />
          <virtual
            name="sleptUnderNets"
            concrete="sleptUnderNets"
            type="dss.vector.solutions.intervention.monitor.Household" />
          <virtual
            name="netsUsed"
            concrete="netsUsed"
            type="dss.vector.solutions.intervention.monitor.Household" />
        </attributes>
        <mdMethod
          name="deleteConcrete"
          label="Delete Concrete"
          returnType="void"
          static="false" />
        <mdMethod
          name="applyAll"
          label="Apply All"
          returnType="void"
          static="false">
          <mdParameter
            name="nets"
            order="0"
            label="Nets"
            type="dss.vector.solutions.intervention.monitor.HouseholdNet[]" />
        </mdMethod>
        <mdMethod
          name="getHouseholdNets"
          label="Get HouseholdNets"
          static="false"
          returnType="dss.vector.solutions.intervention.monitor.HouseholdNet[]" />
      </mdView>

      <mdBusiness
        name="dss.vector.solutions.intervention.monitor.Person"
        label="Person"
        generateController="true"
        description="Surveyed Person">
        <attributes>
          <reference
            name="household"
            label="Household"
            required="true"
            type="dss.vector.solutions.intervention.monitor.Household"
            immutable="true" />
          <char
            name="personId"
            label="Person Id"
            indexType="unique index"
            size="255"
            required="true" />
          <date
            name="dob"
            label="Date of Birth"
            description="Persons date of birth" />
          <reference
            name="sex"
            label="Sex"
            type="dss.vector.solutions.ontology.Term" />
          <boolean
            name="pregnant"
            label="Pregnant"
            description="Flag indicating if the person is pregnant"
            negativeLabel="No"
            positiveLabel="Yes" />
          <boolean
            name="sleptUnderNet"
            label="Did person sleep under net last night"
            description="Flag indicating if the person slept under a net last night"
            negativeLabel="No"
            positiveLabel="Yes" />
          <boolean
            name="haemoglobinMeasured"
            label="Was hemoglobin measured"
            description="Flag indicating if the person had their haemoglobin measured"
            negativeLabel="No"
            positiveLabel="Yes" />
          <decimal
            name="haemoglobin"
            label="Hemoglobin"
            length="3"
            decimal="1"
            description="Hemoglobin value" />
          <reference
            name="anaemiaTreatment"
            label="Anaemia Treatment"
            type="dss.vector.solutions.ontology.Term" />
          <boolean
            name="iron"
            label="Was iron given"
            description="Flag indicating if iron was given"
            negativeLabel="No"
            positiveLabel="Yes" />
          <reference
            name="performedRDT"
            label="Was RDT done"
            type="dss.vector.solutions.ontology.Term" />
          <reference
            name="rdtTreatment"
            label="Treatment given"
            type="dss.vector.solutions.ontology.Term" />
          <reference
            name="bloodslide"
            label="Bloodslide detail"
            type="dss.vector.solutions.ontology.Term" />
          <reference
            name="fever"
            label="Has person had fever in last week?"
            type="dss.vector.solutions.ontology.Term" />
          <reference
            name="feverTreatment"
            label="What treatment was sought for fever?"
            type="dss.vector.solutions.ontology.Term" />
          <reference
            name="malaria"
            label="Was this episode confirmed malaria?"
            type="dss.vector.solutions.ontology.Term" />
          <reference
            name="malariaTreatment"
            label="Was Treatment taken for this episode"
            type="dss.vector.solutions.ontology.Term" />
          <reference
            name="payment"
            label="Payment for treatment"
            type="dss.vector.solutions.ontology.Term" />
        </attributes>
      </mdBusiness>

      <mdTree
        name="dss.vector.solutions.intervention.monitor.PersonRDTResult"
        label="Person RDT Result"
        generateController="false">
        <parent
          cardinality="*"
          name="dss.vector.solutions.intervention.monitor.Person"
          method="Persons" />
        <child
          cardinality="*"
          name="dss.vector.solutions.ontology.Term"
          method="RDTResults" />
      </mdTree>

      <mdTree
        name="dss.vector.solutions.intervention.monitor.HouseholdPerson"
        label="Household Person"
        generateController="false">
        <parent
          cardinality="1"
          name="dss.vector.solutions.intervention.monitor.Household"
          method="Households" />
        <child
          cardinality="*"
          name="dss.vector.solutions.intervention.monitor.Person"
          method="Persons" />
      </mdTree>

      <mdProblem
        name="dss.vector.solutions.intervention.monitor.SprayProblem"
        label="Spray Problem"
        extends="dss.vector.solutions.NotificationProblem">
        <attributes>
          <integer
            name="months"
            label="Months"
            required="true" />
        </attributes>
      </mdProblem>

      <mdProblem
        name="dss.vector.solutions.intervention.monitor.PregnantProblem"
        label="PregnantProblem"
        extends="dss.vector.solutions.NotificationProblem" />

      <mdProblem
        name="dss.vector.solutions.surveillance.RequiredGeoIdProblem"
        label="Required GeoId Problem" />

      <mdProblem
        name="dss.vector.solutions.surveillance.RequiredPeriodTypeProblem"
        label="Required Period Type Problem" />

      <mdProblem
        name="dss.vector.solutions.surveillance.RequiredPeriodProblem"
        label="Required Period Problem" />

      <mdProblem
        name="dss.vector.solutions.surveillance.RequiredYearProblem"
        label="Required Year Problem" />

      <mdProblem
        name="dss.vector.solutions.surveillance.RequiredAgeGroupProblem"
        label="Required Age Group Problem" />
    </create>

    <update>
      <mdController
        name="dss.vector.solutions.intervention.monitor.HouseholdController">
        <mdAction
          name="create"
          isPost="true">
          <mdParameter
            name="dto"
            order="0"
            type="dss.vector.solutions.intervention.monitor.HouseholdView" />
          <create>
            <mdParameter
              name="nets"
              label="Nets"
              order="1"
              type="dss.vector.solutions.intervention.monitor.HouseholdNet[]" />
          </create>
        </mdAction>
        <mdAction
          name="update"
          isPost="true">
          <mdParameter
            name="dto"
            order="0"
            type="dss.vector.solutions.intervention.monitor.HouseholdView" />
          <create>
            <mdParameter
              name="nets"
              label="Nets"
              order="1"
              type="dss.vector.solutions.intervention.monitor.HouseholdNet[]" />
          </create>
        </mdAction>
        <mdAction
          name="newInstance"
          isPost="false">
          <create>
            <mdParameter
              name="surveyId"
              label="Survey Id"
              order="0"
              type="java.lang.String" />
          </create>
        </mdAction>
        <mdAction
          name="cancel"
          isPost="true">
          <mdParameter
            name="dto"
            type="dss.vector.solutions.intervention.monitor.HouseholdView"
            order="0" />
        </mdAction>
        <mdAction
          name="delete"
          isPost="true">
          <mdParameter
            name="dto"
            type="dss.vector.solutions.intervention.monitor.HouseholdView"
            order="0" />
        </mdAction>

      </mdController>
      <mdController
        name="dss.vector.solutions.intervention.monitor.PersonController">
        <mdAction
          name="newInstance">
          <create>
            <mdParameter
              name="householdId"
              label="Household Id"
              order="0"
              type="java.lang.String" />
          </create>
        </mdAction>
      </mdController>
    </update>
  </doIt>
  <undoIt>
    <delete>
      <object
        key="dss.vector.solutions.intervention.monitor.Person"
        type="com.runwaysdk.system.metadata.MdBusiness" />
      <object
        key="dss.vector.solutions.intervention.monitor.HouseholdNet"
        type="com.runwaysdk.system.metadata.MdTree" />
      <object
        key="dss.vector.solutions.intervention.monitor.Household"
        type="com.runwaysdk.system.metadata.MdBusiness" />
      <object
        key="dss.vector.solutions.intervention.monitor.SurveyPoint"
        type="com.runwaysdk.system.metadata.MdBusiness" />
    </delete>
  </undoIt>
</version>
