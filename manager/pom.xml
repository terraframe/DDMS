  <name>Manager</name>
  <description>Manager</description>
  <packaging>jar</packaging>
  <repositories>
    <repository>
      <id>nexus</id>
      <name>nexus</name>
      <url>http://nexus.madrivercode.com/content/repositories/eclipse-3.5.2</url>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
    </repository>
  </repositories>
  <dependencies>
    <dependency>
      <groupId>org.eclipse</groupId>
      <artifactId>swt</artifactId>
      <version>3.557</version>
      <type>jar</type>
      <scope>system</scope>
      <systemPath>/home/jsmethie/workspace/org.eclipse.swt/swt.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>dss.vector.solutions</groupId>
      <artifactId>tomcat-remote-listener</artifactId>
