<xs:complexType name="mdattributeprimitive">
  <xs:complexContent>
    <xs:extension base="mdattributeconcrete">
	  <xs:sequence>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributeboolean">
  <xs:complexContent>
    <xs:extension base="mdattributeprimitive">
	  <xs:sequence>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributenumber">
  <xs:complexContent>
    <xs:extension base="mdattributeprimitive">
	  <xs:sequence>
	    <xs:element name="rejectPositive" type="xs:integer"/>
	    <xs:element name="rejectZero" type="xs:integer"/>
	    <xs:element name="rejectNegative" type="xs:integer"/>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributeint">
  <xs:complexContent>
    <xs:extension base="mdattributenumber">
	  <xs:sequence>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributeinteger">
  <xs:complexContent>
    <xs:extension base="mdattributeint">
	  <xs:sequence>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributelong">
  <xs:complexContent>
    <xs:extension base="mdattributeint">
	  <xs:sequence>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributedec">
  <xs:complexContent>
    <xs:extension base="mdattributenumber">
	  <xs:sequence>
	    <xs:element name="databaseLength" type="xs:integer"/>
	    <xs:element name="databaseDecimal" type="xs:integer"/>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributefloat">
  <xs:complexContent>
    <xs:extension base="mdattributedec">
	  <xs:sequence>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributedouble">
  <xs:complexContent>
    <xs:extension base="mdattributedec">
	  <xs:sequence>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributedecimal">
  <xs:complexContent>
    <xs:extension base="mdattributedec">
	  <xs:sequence>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributeencryption">
  <xs:complexContent>
    <xs:extension base="mdattributeconcrete">
	  <xs:sequence>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributesymmetric">
  <xs:complexContent>
    <xs:extension base="mdattributeencryption">
	  <xs:sequence>
	    <xs:element name="symmetricMethod" type="xs:string"/>
	    <xs:element name="secretKeySize" type="xs:string"/>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributehash">
  <xs:complexContent>
    <xs:extension base="mdattributeencryption">
	  <xs:sequence>
	    <xs:element name="hashMethod" type="xs:string"/>
        <xs:element ref="hashMethod_c" minOccurs="1"/>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributeblob">
  <xs:complexContent>
    <xs:extension base="mdattributeprimitive">
	  <xs:sequence>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributechar">
  <xs:complexContent>
    <xs:extension base="mdattributeprimitive">
	  <xs:sequence>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributecharacter">
  <xs:complexContent>
    <xs:extension base="mdattributechar">
	  <xs:sequence>
	    <xs:element name="databaseSize" type="xs:integer"/>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributetext">
  <xs:complexContent>
    <xs:extension base="mdattributechar">
	  <xs:sequence>
	    <!--
      <xs:element name="databaseRows" type="xs:integer"/>
	    <xs:element name="databaseColumns" type="xs:integer"/>
      -->
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributemoment">
  <xs:complexContent>
    <xs:extension base="mdattributeprimitive">
	  <xs:sequence>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributetime">
  <xs:complexContent>
    <xs:extension base="mdattributemoment">
	  <xs:sequence>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributedate">
  <xs:complexContent>
    <xs:extension base="mdattributemoment">
	  <xs:sequence>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributedatetime">
  <xs:complexContent>
    <xs:extension base="mdattributemoment">
	  <xs:sequence>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributeref">
  <xs:complexContent>
    <xs:extension base="mdattributeconcrete">
	  <xs:sequence>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributefile">
  <xs:complexContent>
    <xs:extension base="mdattributeref">
	  <xs:sequence>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributereference">
  <xs:complexContent>
    <xs:extension base="mdattributeref">
	  <xs:sequence>
	    <xs:element name="mdBusiness" type="xs:string"/>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributeenumeration">
  <xs:complexContent>
    <xs:extension base="mdattributeref">
	  <xs:sequence>
	  	<xs:element name="cacheColumnName" type="xs:string"/>
	    <xs:element name="mdEnumeration" type="xs:string"/>
	    <xs:element name="selectMultiple" type="xs:integer"/>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:complexType name="mdattributestruct">
  <xs:complexContent>
    <xs:extension base="mdattributeconcrete">
	  <xs:sequence>
	    <xs:element name="mdStruct" type="xs:string"/>
	  </xs:sequence>
	</xs:extension>
  </xs:complexContent>
</xs:complexType>

<xs:element name="attributes">
  <xs:complexType>
    <xs:choice maxOccurs="unbounded">
      <xs:element name="attribute" type="attribute"/>
      <xs:element name="reference" type="reference"/>
    </xs:choice>
  </xs:complexType>
</xs:element>

<xs:element name="choices">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="choice" type="choice" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:complexType name="choice">
  <xs:sequence maxOccurs="unbounded">
    <xs:element name="attribute" type="attribute"/>
  </xs:sequence>
</xs:complexType>

<xs:complexType name="attribute">
  <xs:sequence>
    <xs:element name="name" type="xs:string"/>
    <xs:element name="value" type="xs:string"/>
    <xs:element name="definingComponent" type="xs:string"/>
    <xs:element ref="enumeration" minOccurs="0"/>
  </xs:sequence>
</xs:complexType>

<xs:complexType name="reference">
  <xs:sequence>
    <xs:element name="name" type="xs:string"/>
    <xs:element name="value" type="xs:string"/>
  </xs:sequence>
</xs:complexType>

</xs:schema>
