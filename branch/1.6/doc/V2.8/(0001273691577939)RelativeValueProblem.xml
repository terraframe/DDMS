<version
  xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdProblem
        name="dss.vector.solutions.RelativeValueProblem"
        label="Relative Value Problem"
        extends="dss.vector.solutions.NotificationProblem"
        description="[attributeLabel] values must be [relation] [relativeAttributeLabel]">
        <attributes>
          <text
            name="relation"
            label="Relation"
            required="true" />
          <text
            name="relativeAttributeLabel"
            label="Relative Attribute"
            required="true" />
        </attributes>
      </mdProblem>
    </create>
    <update></update>
  </doIt>
  <undoIt>
    <delete></delete>
  </undoIt>
</version>