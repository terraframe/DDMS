<version xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
    
      <mdException
        name="dss.vector.solutions.geo.HierarchyGapException"
        label="Hierarchy Gap Exception"
        description="Thrown when a user tries to define a gap in either the political or spray hierarchy">
      </mdException>

      <mdException
        name="dss.vector.solutions.geo.HierarchyBranchException"
        label="Hierarchy Branch Exception"
        description="Thrown when a user tries to branch the political or spray hierarchy" >
      </mdException>      
    
    </create>
    <update>
    
      <mdBusiness
        name="dss.vector.solutions.ontology.Term">
          <create>
            <mdMethod
              name="getAllTermsForField"
              label="Get All Terms For Field"
              description="Returns all terms, flattened, for the given class and attribute field."
              returnType="dss.vector.solutions.ontology.Term[]"
              static="true">
              <mdParameter
                name="className"
                label="Class Name"
                description="Class Name of the MdClass"
                order="0"
                type="java.lang.String"/>
              <mdParameter
                name="attributeName"
                label="Attribute Name"
                description="Name of the MdAttribute"
                order="1"
                type="java.lang.String"/>
            </mdMethod>
          </create>
      </mdBusiness>        
    
    </update>
    <delete>
    
      <object
        key="dss.vector.solutions.export.GeoEntityExcelView.gazId"
        type="com.runwaysdk.system.metadata.MdAttribute" />
        
      <object
        key="dss.vector.solutions.geo.generated.GeoEntity.gazId"
        type="com.runwaysdk.system.metadata.MdAttribute" />

      <object
        key="dss.vector.solutions.ontology.Term.getAllTermsByAttribute"
        type="com.runwaysdk.system.metadata.MdMethod"/>
          
      </delete>    
  </doIt>
  <undoIt>
  </undoIt>
</version>