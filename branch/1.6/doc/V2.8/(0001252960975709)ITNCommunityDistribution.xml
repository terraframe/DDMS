          <virtual
            name="householdSurname"
            concrete="householdSurname"
            type="dss.vector.solutions.intervention.monitor.ITNCommunityDistribution" />
          <char
            name="householdAddress"
            label="Address"
            size="255"
            required="true" />
          <virtual
            name="residents"
            concrete="residents"
            type="dss.vector.solutions.intervention.monitor.ITNCommunityDistribution" />
          <char
            name="distributionLocation"
            label="Distribution Location"
            size="255"
            required="true" />
          <virtual
