        extends="dss.vector.solutions.export.ControlInterventionExcelView">
        <attributes>
          <virtual
            concrete="vehicleDays"
            type="dss.vector.solutions.intervention.monitor.PersonIntervention" />
        </attributes>
      </mdView>
      <mdView
        name="dss.vector.solutions.export.InsecticideInterventionExcelView"
        label="Insecticide Intervention Excel View"
