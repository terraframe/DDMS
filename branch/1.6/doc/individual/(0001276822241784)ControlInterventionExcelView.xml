        label="Individual Premise Excel View"
        description="Excel view for individual premises"
        extends="dss.vector.solutions.export.ControlInterventionExcelView">
        <attributes>
          <reference
            name="premiseGeoEntity"
            label="Premise Geo Entity"
            type="dss.vector.solutions.geo.generated.GeoEntity"/>
          <virtual
            concrete="visited"
            type="dss.vector.solutions.intervention.monitor.IndividualPremiseVisit" />
