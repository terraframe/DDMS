<version xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdView
        name="dss.vector.solutions.export.ControlInterventionExcelView"
        label="Control Intervention Excel View"
        description="Excel view for control intervention"
        publish="false">
        <attributes>
          <virtual
            concrete="startDate"
            type="dss.vector.solutions.intervention.monitor.ControlIntervention" />
          <virtual
            concrete="endDate"
            type="dss.vector.solutions.intervention.monitor.ControlIntervention" />
          <virtual
            concrete="geoEntity"
