<version xsi:noNamespaceSchemaLocation="../../profiles/version_gis.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>

  
    <create>
    
      <mdInformation
        name="dss.vector.solutions.geo.ChildEntityOverflowInformation"
        label="Child Entity Overflow Page Information"
        description="Thrown when a parent entity has too many children for tree display."
        message="{overflowStart} ... {overflowEnd}">
        <attributes>
          <integer
            name="overflowPage"
            label="Overflow Page"
            required="true" />
          <integer
            name="overflowStart"
            label="Overflow Start"
            required="true" />
          <integer
            name="overflowEnd"
            label="Overflow End"
            required="true" />
          <integer
            name="overflowNumber"
            label="Overflow Number"
            rejectNegative="true"
            required="true" />
          <integer
            name="overflowThreshold"
            label="Overflow Threshold"
            rejectNegative="true"
            rejectZero="false"
            required="true" />
        </attributes>
      </mdInformation>
    
    </create>
    <update>
    
      <mdBusiness
        name="dss.vector.solutions.geo.generated.GeoEntity">
        <create>
          <mdMethod name="getOrderedChildrenPage"
          label="Get Ordered Child Entities"
          description="Gets LocatedIn children ordered by label and page."
          static="false"
          returnType="dss.vector.solutions.geo.GeoEntityViewQuery">
          <mdParameter
            name="typeFilter"
            label="Type Filter"
            description="Name of GeoEntity subclass to filter."
            order="0"
            type="java.lang.String" />
          <mdParameter
            name="pageNumber"
            label="Page Number"
            description="The page number with child entities"
            order="1"
            type="java.lang.Integer"/>
          </mdMethod>
        </create>
      </mdBusiness>
    
    </update>
  </doIt>
  <undoIt>
  <delete>
    
    <object key="dss.vector.solutions.geo.generated.GeoEntity.getOrderedChildrenPage" type="com.runwaysdk.system.metadata.MdMethod"/>
    <object key="dss.vector.solutions.geo.ChildEntityOverflowInformation" type="com.runwaysdk.system.metadata.MdInformation"/>
    
    </delete>
  </undoIt>
</version>