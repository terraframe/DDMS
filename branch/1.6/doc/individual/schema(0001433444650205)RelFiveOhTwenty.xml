<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdException
        name="dss.vector.solutions.report.ReportResourcesException"
        label="report resources exception"
        message="There is a problem with the uploaded BIRT resources.">
      </mdException>
    </create>
    <update>
      <mdBusiness name="dss.vector.solutions.report.ReportItem">
        <create>
          <mdMethod name="uploadResources" returnType="void" static="true">
            <mdParameter name="resourcesIS" label="Resources bundle for report file" order="0" type="java.io.InputStream" />
            <mdParameter name="nameOfResource" label="Name of resource" order="1" type="java.lang.String" />
          </mdMethod>
        </create>
      </mdBusiness>
      <mdController name="dss.vector.solutions.report.ReportItemController">
        <create>
          <mdAction name="uploadResources" isPost="true" label="Upload Resources">
            <mdParameter name="resourcesMFP" label="Resources bundle for report file" order="0" type="com.runwaysdk.controller.MultipartFileParameter" />
          </mdAction>
        </create>
      </mdController>
    </update>
  </doIt>
  <undoIt>
    <delete>
      <object key="dss.vector.solutions.report.ReportItem.uploadResources" type="com.runwaysdk.system.metadata.MdMethod"/>
      <object key="dss.vector.solutions.report.ReportItemController.uploadResources" type="com.runwaysdk.system.metadata.MdAction"/>
      <!-- <object key="dss.vector.solutions.report.ReportResourcesException" type="com.runwaysdk.system.metadata.MdException"/> -->
    </delete>
  </undoIt>
</version>